        document.addEventListener('click', (event) => {
          const profileMenu = document.getElementById('profileMenu');
          const menuContent = document.querySelector('.profile-menu-content');
          
          if (profileMenu && menuContent && !profileMenu.contains(event.target) && menuContent.style.display === 'block') {
            menuContent.style.display = 'none';
          }
        });

        // AI 카드 클릭 시 상세 정보 토글
        function toggleDetails(cardId) {
            const detailSection = document.getElementById(`detail-${cardId}`);
            const allDetails = document.querySelectorAll('.detail-section');
            
            // 다른 모든 상세 정보 닫기
            allDetails.forEach(detail => {
                if (detail.id !== `detail-${cardId}`) {
                    detail.classList.remove('active');
                }
            });
            
            // 클릭된 카드의 상세 정보 토글
            detailSection.classList.toggle('active');

            // 스크롤 애니메이션
            if (detailSection.classList.contains('active')) {
                detailSection.scrollIntoView({ behavior: 'smooth', block: 'start' });
            }
        }

        // 탭 전환 함수
        function switchTab(tabId) {
          const tabContents = document.querySelectorAll('.tab-content');
          const tabs = document.querySelectorAll('.tab-button');
          
          tabContents.forEach(content => content.classList.add('hidden'));
          tabs.forEach(tab => tab.classList.remove('active'));
          
          document.getElementById(tabId).classList.remove('hidden');
          document.querySelector(`[data-tab="${tabId}"]`).classList.add('active');
        }

        // 탭 클릭 이벤트 리스너 설정
        document.addEventListener('DOMContentLoaded', () => {
          document.querySelectorAll('.tab-button').forEach(button => {
            button.addEventListener('click', () => {
              switchTab(button.dataset.tab);
            });
          });
        });

        // 별점 표시 함수
        function getStarRating(rating) {
            const fullStars = Math.floor(rating);
            const hasHalfStar = rating % 1 >= 0.5;
            const emptyStars = 5 - Math.ceil(rating);
            
            return `
                ${'<i class="fas fa-star text-yellow-400"></i>'.repeat(fullStars)}
                ${hasHalfStar ? '<i class="fas fa-star-half-alt text-yellow-400"></i>' : ''}
                ${'<i class="far fa-star text-yellow-400"></i>'.repeat(emptyStars)}
            `;
        }

        // 공유하기 기능
        function shareTool(toolId, toolName) {
            if (navigator.share) {
                navigator.share({
                    title: toolName,
                    text: `${toolName} - AI Curator에서 확인해보세요!`,
                    url: window.location.href
                })
                .catch(error => console.log('공유 실패:', error));
            } else {
