  if (window.__bootstrapInitialized) return; // ì¤‘ë³µ ë°©ì§€
  window.__bootstrapInitialized = true;

  // 1) í…Œë§ˆ ì´ˆê¸°í™”: DOMContentLoaded ì „ì— ê¸°ë³¸ê°’ ë³´ì¥(ì´ë¯¸ theme-managerê°€ ì¦‰ì‹œ ë‹¤í¬ ë³€ìˆ˜ ì ìš©)
  document.addEventListener('DOMContentLoaded', () => {
    try {
      const savedTheme = localStorage.getItem('theme') || 'dark';
      document.documentElement.setAttribute('data-theme', savedTheme);
      document.body.setAttribute('data-theme', savedTheme);
      // í…Œë§ˆ í† ê¸€ ì´ˆê¸°í™”ëŠ” header-menuì—ì„œ SimpleThemeToggleì´ ì²˜ë¦¬
    } catch (e) {}
