        } else {
            alert(errorMessage);
        }
    } finally {
        // ì €ì¥ ë²„íŠ¼ ì¬í™œì„±í™”
        const saveBtn = document.getElementById('saveRecipe');
        if (saveBtn) {
            saveBtn.disabled = false;
            saveBtn.innerHTML = '<i class="fas fa-save mr-2"></i>ë ˆì‹œí”¼ ì €ì¥';
        }
    }
}

