                if (favoriteSnapshot.empty) {
                    if (favoriteToolsContainer) {
                        favoriteToolsContainer.innerHTML = '<div class="text-gray-400 text-center py-8"><i class="fas fa-bookmark text-4xl mb-4 opacity-50"></i><p class="text-lg">ì €ì¥í•œ ë„êµ¬ê°€ ì—†ìŠµë‹ˆë‹¤.</p></div>';
                    }
                } else {
                    favoriteToolsContainer.innerHTML = '';
                    favoriteSnapshot.forEach(docSnap => {
                        const tool = docSnap.data();
                        if (favoriteToolsContainer) {
