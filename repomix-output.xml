    </style>

    <!-- Firebase 초기화는 auth-manager에서 수행 -->
    <script type="module" src="js/auth-manager.js"></script>

    <!-- 공통 모듈 -->
    <script type="module" src="js/db-manager.js"></script>
</head>
<body>
    <div id="header-container"></div>

    <script type="module">
        // ====== 새 필터 로직 시작 ======
        // 검색 쿼리
        let currentSearchQuery = '';
        // 선택된 카테고리들
        let selectedCategories = new Set();
        // 선택된 알파벳 필터
        let selectedAlphabet = '';
        // 전체 AI 도구 데이터 (초기 로드하지 않음)
        let allTools = [];
        // 현재 표시된 도구들
        let currentDisplayedTools = [];

        // 카테고리 클릭 이벤트 등록 (UI만 먼저 준비)
        document.addEventListener('DOMContentLoaded', () => {
            // 초기 상태 설정
            selectedAlphabet = ''; // "전체" 선택
            
            // 카테고리 필터 이벤트
            document.querySelectorAll('.filter-card').forEach(card => {
                card.addEventListener('click', function() {
                    const category = this.dataset.category;
                    if (selectedCategories.has(category)) {
                        selectedCategories.delete(category);
                        this.classList.remove('selected');
                    } else {
                        // 최대 2개까지만 선택 가능
                        if (selectedCategories.size >= 2) {
                            // 이미 2개가 선택된 경우, 첫 번째 선택을 해제하고 새로 선택
                            const firstCategory = Array.from(selectedCategories)[0];
                            selectedCategories.delete(firstCategory);
                            document.querySelector(`[data-category="${firstCategory}"]`).classList.remove('selected');
                        }
                        selectedCategories.add(category);
                        this.classList.add('selected');
                    }
                    
                    // 카테고리 선택 시 알파벳 필터 리셋
                    if (selectedCategories.size > 0) {
                        selectedAlphabet = '';
                        document.querySelectorAll('.alphabet-filter').forEach(f => f.classList.remove('bg-white', 'bg-opacity-20'));
                        // "전체" 선택 상태로 복원
                        document.querySelector('.alphabet-filter[data-letter=""]').classList.add('bg-white', 'bg-opacity-20');
                    }
                    
                    updateSelectedCategoriesCount();
                    applyFilters();
                });
            });

            // 알파벳 필터 이벤트
            document.querySelectorAll('.alphabet-filter').forEach(filter => {
                filter.addEventListener('click', function() {
                    // 이전 선택 해제
                    document.querySelectorAll('.alphabet-filter').forEach(f => f.classList.remove('bg-white', 'bg-opacity-20'));
                    // 현재 선택
                    this.classList.add('bg-white', 'bg-opacity-20');
                    selectedAlphabet = this.dataset.letter;
                    
                    // 알파벳 필터 선택 시 카테고리 필터 리셋
                    if (selectedAlphabet !== '') {
                        selectedCategories.clear();
                        document.querySelectorAll('.filter-card').forEach(card => card.classList.remove('selected'));
                        updateSelectedCategoriesCount();
                    }
                    
                    applyFilters();
                });
            });

            // 우측 상단 Total 버튼 이벤트
            document.getElementById('total-button-top').addEventListener('click', async () => {
                await loadAITools();
                applyFilters();
            });
        });



        // DB 준비 이벤트 후 초기화 (데이터는 로드하지 않음)
        window.addEventListener('dbManagerReady', async (e) => {
            if (e.detail && e.detail.success) {
                console.log('DBManager 준비 완료 - 초기 데이터는 로드하지 않음');
                // 초기에는 데이터를 로드하지 않고, 빈 상태로 시작
                displayNoResults('필터를 선택하거나 Total 버튼을 클릭하여 AI 도구를 확인하세요.');
                updateFilterCount(0);
            } else {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(new Error(e.detail && e.detail.error || 'DBManager 초기화 실패'), 'DBManager 초기화');
                } else {
                    console.error('AI 도구 Firestore 로드 중 오류:', e.detail && e.detail.error);
                }
            }
        });

        // 이미 초기화된 경우에도 초기 데이터는 로드하지 않음
        if (window.dbManager && window.dbManager.isInitialized) {
            console.log('DBManager 이미 초기화됨 - 초기 데이터는 로드하지 않음');
            // 초기에는 데이터를 로드하지 않고, 빈 상태로 시작
            displayNoResults('필터를 선택하거나 Total 버튼을 클릭하여 AI 도구를 확인하세요.');
            updateFilterCount(0);
        }

        // 필터 적용 함수
        function applyFilters() {
            // 데이터가 로드되지 않은 경우 빈 결과 표시
            if (allTools.length === 0) {
                displayNoResults('필터를 선택하거나 Total 버튼을 클릭하여 AI 도구를 확인하세요.');
                updateFilterCount(0);
                return;
            }

            let filtered = [...allTools];

            // 검색어 필터 적용
            if (currentSearchQuery) {
                const query = currentSearchQuery.toLowerCase();
                filtered = filtered.filter(tool => {
                    const name = (tool.name || '').toLowerCase();
                    const description = (tool.description || '').toLowerCase();
                    const tags = (tool.tags || []).map(tag => 
                        typeof tag === 'object' ? tag.name : tag
                    ).join(' ').toLowerCase();
                    
                    return name.includes(query) || 
                           description.includes(query) || 
                           tags.includes(query);
                });
            }

            // 카테고리 필터와 알파벳 필터는 상호 배타적 (동시 적용 불가)
            // 카테고리 필터 적용 (AND 조건)
            if (selectedCategories.size > 0) {
                filtered = filtered.filter(tool => {
                    const toolCategories = [tool.primaryCategory, ...(tool.categories || [])].filter(Boolean);
                    return Array.from(selectedCategories).every(cat => toolCategories.includes(cat));
                });
            }
            
            // 알파벳 필터 적용 (카테고리 필터가 선택되지 않은 경우에만)
            if (selectedAlphabet && selectedCategories.size === 0) {
                filtered = filtered.filter(tool => {
                    const toolName = tool.name || '';
                    return toolName.toUpperCase().startsWith(selectedAlphabet);
                });
            }

            // 정렬 적용
            const sortOption = document.getElementById('sort-option');
            if (sortOption) {
                const sortValue = sortOption.value;
                switch (sortValue) {
                    case 'popularity':
                        filtered.sort((a, b) => (b.rating || 0) - (a.rating || 0));
                        break;
                    case 'rating':
                        filtered.sort((a, b) => (b.rating || 0) - (a.rating || 0));
                        break;
                    case 'newest':
                        filtered.sort((a, b) => new Date(b.createdAt || 0) - new Date(a.createdAt || 0));
                        break;
                    case 'name':
                        filtered.sort((a, b) => (a.name || '').localeCompare(b.name || ''));
                        break;
                }
            }

            // 결과 개수 표시
            updateFilterCount(filtered.length);
            
            // 결과 설명 업데이트
            updateFilterDescription();
            
            // 결과 표시
            currentDisplayedTools = filtered;
            displayTools(filtered);
        }
        
        // 도구 목록 표시 함수
        function displayTools(tools) {
            const container = document.getElementById('tools-container');
            
            if (!tools || tools.length === 0) {
                displayNoResults('검색 결과가 없습니다');
                return;
            }
            
            container.innerHTML = tools.map(tool => `
                <div class="tool-card glass-effect rounded-xl p-6 cursor-pointer transition-all duration-300 hover:scale-105 hover:shadow-lg" 
                     onclick="displayToolDetails('${tool.id}')" 
                     style="background: var(--card-bg); border: 1px solid var(--border-color);">
                    <div class="flex items-start space-x-4">
                        <!-- 도구 이미지 -->
                        <div class="flex-shrink-0">
                            <div class="w-16 h-16 rounded-lg overflow-hidden bg-gray-700 flex items-center justify-center">
                                <img src="${tool.imageUrl || tool.logoUrl || window.DEFAULT_IMAGE}" 
                                     alt="${tool.name}" 
                                     class="w-full h-full object-cover"
                                     onerror="window.handleImageError(this)">
                            </div>
                        </div>
                        
                        <!-- 도구 정보 -->
                        <div class="flex-1 min-w-0">
                            <h3 class="text-lg font-semibold text-white mb-2 line-clamp-2">${tool.name}</h3>
                            <p class="text-gray-300 text-sm mb-3 line-clamp-3">${tool.description || '설명이 없습니다'}</p>
                            
                            <!-- 태그 -->
                            <div class="flex flex-wrap gap-2 mb-3">
                                ${(tool.tags || []).slice(0, 3).map(tag => {
                                    const tagName = typeof tag === 'object' ? tag.name : tag;
                                    return `<span class="px-2 py-1 bg-indigo-600 bg-opacity-20 text-indigo-300 text-xs rounded-full">${tagName}</span>`;
                                }).join('')}
                            </div>
                            
                            <!-- 평점 및 카테고리 -->
                            <div class="flex items-center justify-between text-sm text-gray-400">
                                <div class="flex items-center space-x-2">
                                    <span class="flex items-center">
                                        <i class="fas fa-star text-yellow-400 mr-1"></i>
                                        ${tool.rating ? tool.rating.toFixed(1) : 'N/A'}
                                    </span>
                                    <span>•</span>
                                    <span>${tool.primaryCategory || '카테고리 없음'}</span>
                                </div>
                                <div class="flex items-center space-x-1">
                                    <i class="fas fa-heart text-red-400"></i>
                                    <span>${tool.favorites || 0}</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            `).join('');
        }

        // 필터 카운트 업데이트
        function updateFilterCount(count) {
            const countElem = document.getElementById('filtered-tools-count');
            if (countElem) countElem.textContent = count;
        }
        
        // 선택된 카테고리 수 업데이트
        function updateSelectedCategoriesCount() {
            const countElem = document.getElementById('selected-categories-count');
            if (countElem) countElem.textContent = selectedCategories.size;
        }

        // 필터 설명 업데이트 (검색어 포함)
        function updateFilterDescription() {
            const descElem = document.getElementById('filter-description');
            if (descElem) {
                let description = '';
                
                if (currentSearchQuery) {
                    description += `"${currentSearchQuery}" 검색 결과`;
                }
                
                if (selectedCategories.size > 0) {
                    // 카테고리 필터만 적용된 경우
                    if (description) description += ' + ';
                    const categoryNames = Array.from(selectedCategories).map(getCategoryDisplayName);
                    description += categoryNames.join(' 및 ') + ' 모두 가능한';
                } else if (selectedAlphabet) {
                    // 알파벳 필터만 적용된 경우
                    if (description) description += ' + ';
                    description += `'${selectedAlphabet}'로 시작하는`;
                }
                
                if (!description) description = '전체';
                
                descElem.textContent = description;
            }
        }

        // 카테고리 한글명 변환
        function getCategoryDisplayName(category) {
            const map = {
                'text-generation': '텍스트 생성',
                'image-generation': '이미지 생성',
                'code-assistant': '코드 어시스턴트',
                'audio-generation': '오디오 생성',
                'video-generation': '비디오 생성',
                'data-analysis': '데이터 분석',
                'marketing': '마케팅',
                'education': '교육',
            };
            return map[category] || category;
        }
        // ====== 새 필터 로직 끝 ======


        
        // 기본 이미지 URL 설정
        window.DEFAULT_IMAGE = 'assets/icons/code-icon1.png';
        
        // 이미지 로드 실패 시 처리하는 함수
        window.handleImageError = function(img) {
            img.onerror = null; // 무한 루프 방지
            img.src = window.DEFAULT_IMAGE;
        };

        // 유효한 이미지 URL 반환 함수
        function getValidImageUrl(tool) {
            return tool.imageUrl || tool.logoUrl || tool.summary?.imageUrl || window.DEFAULT_IMAGE;
        }
        
        // 별점 표시 함수
        function getStarRating(rating) {
            const fullStars = Math.floor(rating);
            const hasHalfStar = rating % 1 >= 0.5;
            const emptyStars = 5 - fullStars - (hasHalfStar ? 1 : 0);
            
            let stars = '';
            for (let i = 0; i < fullStars; i++) {
                stars += '<i class="fas fa-star text-yellow-400"></i>';
            }
            if (hasHalfStar) {
                stars += '<i class="fas fa-star-half-alt text-yellow-400"></i>';
            }
            for (let i = 0; i < emptyStars; i++) {
                stars += '<i class="fas fa-star text-gray-400"></i>';
            }
            return stars;
        }
        
        // 도구 상세 정보 표시 함수를 전역 스코프로 이동 (Firestore 방식)
        window.displayToolDetails = async function(toolId) {
            try {
                window.currentToolId = toolId;
                // Firestore 방식으로 변경 (dbManager 사용)
                const tool = await window.dbManager.getToolDetails(toolId);
                if (tool) {
                    // 상세 정보 섹션 표시
                    const detailSection = document.getElementById('ai-detail-section');
                    detailSection.classList.remove('hidden');

                    // 스크롤 이동
                    detailSection.scrollIntoView({ behavior: 'smooth', block: 'start' });

                    // 기본 정보 업데이트
                    document.getElementById('detail-tool-name').textContent = tool.name || '이름 없음';
                    document.getElementById('detail-tool-description').textContent = tool.description || '설명 없음';
                    
                    // 이미지 업데이트
                    const toolImage = document.getElementById('tool-image-summary');
                    toolImage.innerHTML = `<img src="${getValidImageUrl(tool)}" alt="${tool.name}" class="w-full h-full object-cover rounded-lg" onerror="window.handleImageError(this)">`;

                    // 평점 표시
                    const ratingSection = document.getElementById('rating-section');
                    if (tool.rating) {
                        ratingSection.innerHTML = `
                            <div class="flex items-center mb-4">
                                <div class="rating-stars mr-2">
                                    ${getStarRating(tool.rating)}
                                </div>
                                <span class="text-xl">${tool.rating.toFixed(1)}</span>
                            </div>
                        `;
                    } else {
                        ratingSection.innerHTML = '<p class="text-gray-400">평점 정보 없음</p>';
                    }

                    // 태그 표시
                    const tagsContainer = document.getElementById('tags-container');
                    if (tool.tags?.length > 0) {
                        const tagColors = [
                            'bg-blue-600 text-blue-100',
                            'bg-green-600 text-green-100',
                            'bg-purple-600 text-purple-100',
                            'bg-pink-600 text-pink-100',
                            'bg-yellow-600 text-yellow-100',
                            'bg-red-600 text-red-100',
                            'bg-indigo-600 text-indigo-100',
                            'bg-teal-600 text-teal-100',
                            'bg-orange-600 text-orange-100'
                        ];
                        tagsContainer.innerHTML = tool.tags.map((tag, idx) => {
                            const colorClass = tagColors[idx % tagColors.length];
                            return `<span class="tag ${colorClass}">${tag}</span>`;
                        }).join('');
                    } else {
                        tagsContainer.innerHTML = '<span class="text-gray-400">태그 정보 없음</span>';
                    }

                    // 웹사이트 링크
                    const websiteLink = document.getElementById('website-link');
                    if (tool.websiteUrl) {
                        websiteLink.href = tool.websiteUrl;
                        websiteLink.style.display = 'inline-flex';
                    } else {
                        websiteLink.style.display = 'none';
                    }

                    // 개요 탭 내용 (summary.longDescription 우선, 없으면 description)
                    const overviewContent = document.getElementById('overview-content');
                    overviewContent.innerHTML = (tool.summary?.longDescription || tool.description || '상세 설명이 없습니다.');

                    // 장점/단점 (summary.pros, summary.cons)
                    const prosContent = document.getElementById('pros-content');
                    const consContent = document.getElementById('cons-content');
                    prosContent.innerHTML = Array.isArray(tool.summary?.pros) && tool.summary.pros.length > 0
                        ? tool.summary.pros.map(pro => `
                            <div class="pros-cons-item pros">
                                <i class="fas fa-check-circle"></i>
                                ${pro}
                            </div>
                        `).join('')
                        : '<p class="text-gray-400">장점 정보가 없습니다.</p>';
                    
                    consContent.innerHTML = Array.isArray(tool.summary?.cons) && tool.summary.cons.length > 0
                        ? tool.summary.cons.map(con => `
                            <div class="pros-cons-item cons">
                                <i class="fas fa-times-circle"></i>
                                ${con}
                            </div>
                        `).join('')
                        : '<p class="text-gray-400">단점 정보가 없습니다.</p>';

                    // 가격 정보
                    const pricingContent = document.getElementById('pricing-content');
                    if (tool.pricing && tool.pricing.length > 0) {
                        pricingContent.innerHTML = tool.pricing.map(plan => `
                            <div class="pricing-plan">
                                <h4 class="font-semibold">${plan.name || '플랜'}</h4>
                                <p class="text-lg font-bold">${plan.price || '가격 정보 없음'}</p>
                                <p class="text-sm text-gray-400">${plan.description || ''}</p>
                            </div>
                        `).join('');
                    } else {
                        pricingContent.innerHTML = '<p class="text-gray-400">가격 정보가 없습니다.</p>';
                    }

                    // 사용자 리뷰
                    const reviewsContent = document.getElementById('reviews-content');
                    if (tool.reviews && tool.reviews.length > 0) {
                        reviewsContent.innerHTML = tool.reviews.map(review => `
                            <div class="review-item">
                                <div class="flex items-center mb-2">
                                    <div class="rating-stars mr-2">
                                        ${getStarRating(review.rating || 0)}
                                    </div>
                                    <span class="text-sm text-gray-400">${review.date || ''}</span>
                                </div>
                                <p class="text-sm">${review.comment || ''}</p>
                            </div>
                        `).join('');
                    } else {
                        reviewsContent.innerHTML = '<p class="text-gray-400">리뷰가 없습니다.</p>';
                    }

                    // 저장 버튼 상태 확인
                    if (window.auth && window.auth.currentUser) {
                        checkSaveStatus(toolId);
                    }
                } else {
                    if (window.CommonUtils) {
                        window.CommonUtils.showError('도구 정보를 찾을 수 없습니다.');
                    } else {
                        alert('도구 정보를 찾을 수 없습니다.');
                    }
                }
            } catch (error) {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(error, '도구 상세 정보 표시');
                } else {
                    console.error('도구 상세 정보 표시 오류:', error);
                    alert('도구 정보를 불러오는 중 오류가 발생했습니다.');
                }
            }
        };

        // 탭 설정 함수
        function setupTabs() {
            const tabButtons = document.querySelectorAll('.tab-button');
            const tabContents = document.querySelectorAll('.tab-content');

            tabButtons.forEach(button => {
                button.addEventListener('click', () => {
                    const tabId = button.getAttribute('data-tab');
                    
                    tabButtons.forEach(btn => btn.classList.remove('active'));
                    button.classList.add('active');
                    
                    tabContents.forEach(content => content.classList.remove('active'));
                    document.getElementById(tabId).classList.add('active');
                });
            });
        }

        // 도구 카드 클릭 이벤트 리스너 설정
        function setupToolCardListeners() {
            document.querySelectorAll('.tool-card').forEach(card => {
                card.addEventListener('click', function() {
                    const toolId = this.dataset.toolId;
                    displayToolDetails(toolId);
                });
            });
        }

        // 페이지 로드 시 실행: UI 탭만 먼저 준비
        document.addEventListener('DOMContentLoaded', () => {
            setupTabs();
        });

        // DB 준비되면 데이터 로드
        window.addEventListener('dbManagerReady', async (e) => {
            if (e.detail && e.detail.success) {
                try {
                    await loadAITools();
                } catch (err) {
                    if (window.CommonUtils) {
                        window.CommonUtils.handleError(err, 'AI 도구 로드');
                    } else {
                        console.error('AI 도구 로드 중 오류:', err);
                    }
                }
            } else {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(new Error(e.detail && e.detail.error || 'DBManager 초기화 실패'), 'DBManager 초기화');
                } else {
                    console.error('AI 도구 Firestore 로드 중 오류:', e.detail && e.detail.error);
                }
            }
        });

        // 이미 초기화된 경우 즉시 로드
        if (window.dbManager && window.dbManager.isInitialized) {
            loadAITools().catch(err => {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(err, 'AI 도구 로드');
                } else {
                    console.error('AI 도구 로드 중 오류:', err);
                }
            });
        }

        // 전역 객체는 auth-manager에서 설정됨

       
        // 중복된 함수들 제거됨 - 새로운 필터 시스템 사용

       

        // AI 도구 로드 함수 (Total 버튼 클릭 시에만 사용)
        async function loadAITools() {
            try {
                console.log("AI 도구 로드 시작");
                
                // DBManager를 통해 도구 데이터 가져오기
                allTools = await window.dbManager.loadAITools();
                
                if (!allTools || allTools.length === 0) {
                    console.log("데이터가 없습니다");
                    return;
                }

                console.log("로드된 도구 수:", allTools.length);
                


                // 실시간 업데이트 설정
                window.dbManager.setupRealtimeUpdates((updatedTools) => {
                    allTools = updatedTools;
                    applyFilters();
                });

            } catch (error) {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(error, 'AI 도구 로드');
                } else {
                    console.error("AI 도구 로드 중 오류 발생:", error);
                }
            }
        }

        // 중복된 함수들 제거됨 - 새로운 필터 시스템 사용

        // 결과 없음 표시 함수
        function displayNoResults(message = '검색 결과가 없습니다') {
            const container = document.getElementById('tools-container');
            container.innerHTML = `
                <div class="no-results col-span-full text-center py-12">
                    <i class="fas fa-search text-4xl text-gray-400 mb-4"></i>
                    <h3 class="text-xl text-gray-300 mb-2">${message}</h3>
                    <p class="text-gray-500">필터를 조정하거나 Total 버튼을 클릭해보세요</p>
                </div>
            `;
            // detail 정보 영역도 숨김
            hideToolDetails();
        }
        
        // 도구 상세 정보 숨기기 함수
        function hideToolDetails() {
            const detailSection = document.getElementById('ai-detail-section');
            if (detailSection) {
                detailSection.classList.add('hidden');
            }
        }

        // 로딩 상태 표시 함수
        function showLoading(container) {
            container.innerHTML = `
                <div class="text-center py-12 col-span-full">
                    <div class="inline-block animate-spin rounded-full h-12 w-12 border-t-2 border-b-2 border-indigo-500 mb-4"></div>
                    <p class="text-xl text-gray-300">AI 도구를 불러오는 중...</p>
                    <p class="text-sm text-gray-400 mt-2">잠시만 기다려주세요</p>
                </div>
            `;
        }

        // 에러 처리 함수
        function handleError(container, error) {
            console.error(error);
            container.innerHTML = `
                <div class="text-center py-12 col-span-full">
                    <div class="bg-red-900 bg-opacity-20 rounded-lg p-6 max-w-lg mx-auto">
                        <i class="fas fa-exclamation-circle text-red-500 text-4xl mb-4"></i>
                        <p class="text-xl text-red-500 mb-4">오류가 발생했습니다</p>
                        <p class="text-gray-400 mb-8">${error.message}</p>
                        <button onclick="location.reload()" 
                                class="bg-indigo-600 hover:bg-indigo-700 text-white font-bold py-3 px-6 rounded-lg 
                                       transition duration-200 ease-in-out transform hover:scale-105">
                            <i class="fas fa-redo-alt mr-2"></i>다시 시도
                        </button>
                    </div>
                </div>
            `;
        }

        // 이벤트 리스너 설정
        document.addEventListener('DOMContentLoaded', async function() {
            try {
                // 초기 선택된 카테고리 수 설정
                updateSelectedCategoriesCount();
                
                // URL 쿼리 파라미터에서 키워드 추출
                const urlParams = new URLSearchParams(window.location.search);
                const keyword = urlParams.get('keyword');
                
                if (keyword) {
                    // 검색창에 키워드 입력
                    const searchInput = document.getElementById('search-input');
                    searchInput.value = decodeURIComponent(keyword);
                    currentSearchQuery = decodeURIComponent(keyword);
                    // 검색창에 포커스
                    searchInput.focus();
                    // 검색 실행
                    applyFilters();
                }

                // 검색 입력 이벤트
                const searchInput = document.getElementById('search-input');
                const searchButton = document.getElementById('search-button');
                const searchClear = document.getElementById('search-clear');

                if (searchInput) {
                    searchInput.addEventListener('input', () => {
                        currentSearchQuery = searchInput.value.trim();
                        if (!currentSearchQuery) {
                            applyFilters();
                        }
                    });

                    searchInput.addEventListener('keypress', (e) => {
                        if (e.key === 'Enter') {
                            applyFilters();
                        }
                    });
                }

                if (searchButton) {
                    searchButton.addEventListener('click', applyFilters);
                }
                
                if (searchClear) {
                    searchClear.addEventListener('click', () => {
                        searchInput.value = '';
                        currentSearchQuery = '';
                        applyFilters();
                    });
                }

                // 정렬 옵션 변경 이벤트
                const sortOption = document.getElementById('sort-option');
                if (sortOption) {
                    sortOption.addEventListener('change', applyFilters);
                }
                
            } catch (error) {
                console.error("페이지 초기화 실패:", error);
                const container = document.getElementById('tools-container');
                if (container) {
                    handleError(container, error);
                }
            }
        });

    </script>


