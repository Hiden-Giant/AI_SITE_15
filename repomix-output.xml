        // ë¦¬ë·° ìš”ì†Œ ìƒì„±
        function createReviewElement(reviewData) {
            const reviewDiv = document.createElement('div');
            reviewDiv.className = 'review-card';
            
            const stars = Array(5).fill().map((_, i) => 
                i < reviewData.rating ? 
