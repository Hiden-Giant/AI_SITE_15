    <script type="module">
        // 전역 변수 초기화
        let selectedCategories = new Set();
        let currentSearchQuery = '';
        let popularTools = []; // 인기 도구만 저장
        let allTools = []; // 전체 도구는 지연 로딩
        
        // 기본 이미지 URL 설정
        window.DEFAULT_IMAGE = 'assets/icons/code-icon1.png';
        
        // DBManager 초기화 완료 이벤트 리스너
        window.addEventListener('dbManagerReady', async () => {
            try {
                // dbManager가 준비되었는지 확인
                if (!window.dbManager || !window.dbManager.isInitialized) {
                    throw new Error('DBManager가 아직 초기화되지 않았습니다.');
                }
                
                console.log('DBManager 상태 확인:', window.dbManager.getState());
                
                // 인기 도구만 불러오기 (6개)
                popularTools = await window.dbManager.loadPopularAITools(6, 4.5);
                console.log('인기 AI 도구 데이터 로드 완료:', popularTools.length);
                
                // 인기 도구 즉시 표시
                loadPopularTools();
                
                // 로딩 완료 표시
                showLoadingComplete();
                
            } catch (error) {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(error, '인기 AI 도구 데이터 로드');
                } else {
                    console.error('인기 AI 도구 데이터 로드 실패:', error);
                }
                showLoadingError();
            }
        });
        
        // 전체 도구 로드 완료 이벤트 리스너 (지연 로딩)
        window.addEventListener('allToolsLoaded', (event) => {
            console.log('전체 AI 도구 로드 완료:', event.detail.count);
            allTools = window.dbManager.allTools || [];
            
            // 전체 도구 로드 완료 표시
            showAllToolsLoaded();
        });
        
        // 로딩 완료 표시
        function showLoadingComplete() {
            const loadingElement = document.getElementById('loadingIndicator');
            if (loadingElement) {
                loadingElement.innerHTML = '<div class="text-green-400 text-center"><i class="fas fa-check-circle mr-2"></i>인기 도구 로딩 완료</div>';
                setTimeout(() => {
                    loadingElement.style.display = 'none';
                }, 2000);
            }
        }
        
        // 로딩 에러 표시
        function showLoadingError() {
            const loadingElement = document.getElementById('loadingIndicator');
            if (loadingElement) {
                loadingElement.innerHTML = '<div class="text-red-400 text-center"><i class="fas fa-exclamation-triangle mr-2"></i>로딩 실패</div>';
            }
        }
        
        // 전체 도구 로드 완료 표시
        function showAllToolsLoaded() {
            const loadingElement = document.getElementById('loadingIndicator');
            if (loadingElement) {
                loadingElement.innerHTML = '<div class="text-blue-400 text-center"><i class="fas fa-info-circle mr-2"></i>모든 도구 로딩 완료</div>';
                setTimeout(() => {
                    loadingElement.style.display = 'none';
                }, 2000);
            }
        }
        
        // 이미지 로드 실패 시 처리하는 함수 - 개선된 버전
        window.handleImageError = function(img) {
            img.onerror = null; // 무한 루프 방지
            
            // 기본 이미지로 대체
            if (img.src !== window.DEFAULT_IMAGE) {
                console.log(`이미지 로드 실패, 기본 이미지로 대체: ${img.src}`);
                img.src = window.DEFAULT_IMAGE;
            }
        };

        // getStorageUrl 함수 추가 - Firebase Storage URL 생성
        function getStorageUrl(fileName) {
            if (!fileName || typeof fileName !== 'string' || fileName.trim() === '') {
                console.warn('getStorageUrl: 유효하지 않은 fileName:', fileName);
                return window.DEFAULT_IMAGE;
            }
            
            try {
                // Firebase Storage URL 생성
                const baseUrl = 'https://firebasestorage.googleapis.com/v0/b/ai-tools-data-b2b7b.firebaseapp.com/o/ai_logos%2F';
                const encodedFileName = encodeURIComponent(fileName.trim());
                const token = '4b780bb4-d39d-4195-8c73-4766eda6ad6b';
                
                const fullUrl = `${baseUrl}${encodedFileName}?alt=media&token=${token}`;
                console.log(`Firebase Storage URL 생성: ${fullUrl}`);
                
                return fullUrl;
            } catch (error) {
                console.error('getStorageUrl 오류:', error);
                return window.DEFAULT_IMAGE;
            }
        }
        
        // getValidImageUrl 함수 추가 - 이미지 표시 문제 해결
        function getValidImageUrl(tool) {
            // 1. logoUrl이 있으면 우선 사용
            if (tool.logoUrl && typeof tool.logoUrl === 'string' && tool.logoUrl.trim() !== '') {
                return tool.logoUrl;
            }
            
            // 2. logoFileName이 있으면 Firebase Storage URL 생성
            if (tool.logoFileName && typeof tool.logoFileName === 'string' && tool.logoFileName.trim() !== '') {
                return getStorageUrl(tool.logoFileName);
            }
            
            // 3. imageUrl이 있으면 사용
            if (tool.imageUrl && typeof tool.imageUrl === 'string' && tool.imageUrl.trim() !== '') {
                return tool.imageUrl;
            }
            
            // 4. image가 있으면 사용
            if (tool.image && typeof tool.image === 'string' && tool.image.trim() !== '') {
                return tool.image;
            }
            
            // 5. 모든 이미지가 없으면 기본 이미지 사용
            console.log(`도구 "${tool.name}"의 이미지를 찾을 수 없어 기본 이미지를 사용합니다.`, tool);
            return window.DEFAULT_IMAGE;
        }

        // 인기 도구 로드 함수 - 최적화된 버전
        async function loadPopularTools() {
            try {
                const popularToolsContainer = document.getElementById('popularToolsContainer');
                if (!popularToolsContainer) return;

                // 인기 도구가 없으면 기본 메시지 표시
                if (!popularTools || popularTools.length === 0) {
                    popularToolsContainer.innerHTML = `
                        <div class="text-center text-gray-400 py-8">
                            <i class="fas fa-spinner fa-spin text-2xl mb-4"></i>
                            <p>인기 도구를 불러오는 중...</p>
                        </div>
                    `;
                    return;
                }

                // 인기 도구 표시 (최대 6개)
                const displayTools = popularTools.slice(0, 6);
                
                popularToolsContainer.innerHTML = displayTools.map(tool => `
                    <div class="bg-gray-800/50 rounded-lg p-4 flex items-start gap-4 cursor-pointer hover:bg-gray-700/50 transition-all duration-300" onclick="window.goToToolDetail('${tool.id}')">
                        <div class="flex-shrink-0">
                            <img src="${getValidImageUrl(tool)}" 
                                 alt="${tool.name}"
                                 class="w-12 h-12 rounded-lg object-cover"
                                 onerror="window.handleImageError(this)"
                                 loading="lazy">
                        </div>
                        <div class="flex-grow">
                            <div class="flex items-center justify-between">
                                <h3 class="text-lg font-semibold">${tool.name}</h3>
                                <span class="text-blue-400 text-sm" data-i18n="ai_generated">AI 생성</span>
                            </div>
                            <p class="text-gray-400 text-sm mt-1">${tool.description || ''}</p>
                            <div class="flex items-center justify-between mt-2">
                                <div class="flex items-center">
                                    <div class="flex text-yellow-400">
                                        ${getStarRating(tool.rating || 0)}
                                    </div>
                                    <span class="ml-2 text-gray-400">${tool.rating || 0}</span>
                                </div>
                                <span class="text-gray-400" data-i18n="free_paid">무료/유료</span>
                            </div>
                        </div>
                    </div>
                `).join('');
                
                console.log(`인기 도구 ${displayTools.length}개 표시 완료`);
                
            } catch (error) {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(error, '인기 도구 로드');
                } else {
                    console.error('인기 도구 로드 중 오류:', error);
                }
            }
        }

        // 별점 HTML 생성 함수
        function getStarRating(rating) {
            const fullStars = Math.floor(rating);
            const hasHalfStar = rating % 1 >= 0.5;
            let stars = '';
            
            for (let i = 0; i < fullStars; i++) {
                stars += '<i class="fas fa-star"></i>';
            }
            if (hasHalfStar) {
                stars += '<i class="fas fa-star-half-alt"></i>';
            }
            const emptyStars = 5 - Math.ceil(rating);
            for (let i = 0; i < emptyStars; i++) {
                stars += '<i class="far fa-star"></i>';
            }
            
            return stars;
        }

        // 도구 상세 페이지로 이동하는 함수
        window.goToToolDetail = function(toolId) {
            if (!toolId) {
                console.warn('도구 ID가 없습니다.');
                return;
            }
            window.location.href = `detail_sp.html?id=${encodeURIComponent(toolId)}`;
        };

        // 탭 설정 함수
        function setupTabs() {
            const tabButtons = document.querySelectorAll('[data-tab]');
            const tabContents = document.querySelectorAll('[data-tab-content]');

            tabButtons.forEach(button => {
                button.addEventListener('click', () => {
                    const tabId = button.getAttribute('data-tab');
                    
                    // 모든 탭 버튼에서 active 클래스 제거
                    tabButtons.forEach(btn => btn.classList.remove('active'));
                    // 클릭된 탭 버튼에 active 클래스 추가
                    button.classList.add('active');
                    
                    // 모든 탭 콘텐츠 숨기기
                    tabContents.forEach(content => {
                        content.style.display = 'none';
                    });
                    
                    // 선택된 탭 콘텐츠 표시
                    const selectedContent = document.querySelector(`[data-tab-content="${tabId}"]`);
                    if (selectedContent) {
                        selectedContent.style.display = 'block';
                    }
                });
            });

            // 첫 번째 탭을 기본으로 활성화
            if (tabButtons.length > 0) {
                tabButtons[0].click();
            }
        }

        // 페이지 로드 시 실행
        document.addEventListener('DOMContentLoaded', () => {
            // 초기화 함수 호출
            setupTabs();
        });

        // 카테고리 이동 함수
        window.navigateToCategory = function(categoryId) {
            if (!categoryId) {
                console.warn('카테고리 ID가 없습니다.');
                return;
            }
            // URL 파라미터 생성
            const params = new URLSearchParams();
            params.append('category', categoryId);
            
            // filter_search.html 페이지로 이동
            window.location.href = `filter_search.html?${params.toString()}`;
        };

        // 검색 처리 함수
        window.handleSearch = function() {
            const searchInput = document.getElementById('keyword-search-input');
            const keyword = searchInput.value.trim();
            if (keyword) {
                window.location.href = `filter_search.html?keyword=${encodeURIComponent(keyword)}`;
            }
        };

        // 엔터 키 처리 함수
        window.handleKeyPress = function(event) {
            if (event.key === 'Enter') {
                window.handleSearch();
            }
        };

        // 필터 검색 처리 함수
        window.handleFilterSearch = function() {
            // 선택된 카테고리 값 가져오기
            const categorySelect = document.getElementById('categorySelect');
            const selectedCategory = categorySelect.value;
            
            // 체크된 가격 필터들 확인
            const freeChecked = document.getElementById('freeCheck').checked;
            const premiumChecked = document.getElementById('premiumCheck').checked;
            const subscriptionChecked = document.getElementById('subscriptionCheck').checked;

            // 선택된 가격 타입들을 배열로 모으기
            const selectedPriceTypes = [];
            if (freeChecked) selectedPriceTypes.push('free');
            if (premiumChecked) selectedPriceTypes.push('premium');
            if (subscriptionChecked) selectedPriceTypes.push('subscription');
            
            // URL 파라미터 생성
            const params = new URLSearchParams();
            
            // 카테고리가 선택된 경우 추가
            if (selectedCategory) {
                params.append('category', selectedCategory);
            }
            
            // 가격 필터가 선택된 경우 추가
            if (selectedPriceTypes.length > 0) {
                params.append('price_types', selectedPriceTypes.join(','));
            }
            
            // 기본 최소 평점 추가
            params.append('min_rating', '4.5');
            
            // filter_search.html 페이지로 이동
            window.location.href = `filter_search.html?${params.toString()}`;
        };

        // 이미지 지연 로딩 최적화
        function setupImageLazyLoading() {
            const images = document.querySelectorAll('img[loading="lazy"]');
            
            if ('IntersectionObserver' in window) {
                const imageObserver = new IntersectionObserver((entries, observer) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            const img = entry.target;
                            img.src = img.dataset.src || img.src;
                            img.classList.remove('lazy');
                            imageObserver.unobserve(img);
                        }
                    });
                });
                
                images.forEach(img => {
                    imageObserver.observe(img);
                });
            }
        }
        
        // 스크롤 기반 지연 로딩
        function setupScrollBasedLazyLoading() {
            let isLoading = false;
            
            function handleScroll() {
                if (isLoading) return;
                
                const scrollPosition = window.scrollY + window.innerHeight;
                const documentHeight = document.documentElement.scrollHeight;
                
                // 스크롤이 하단에 가까워지면 추가 데이터 로드
                if (scrollPosition >= documentHeight - 1000) {
                    loadMoreTools();
                }
            }
            
            window.addEventListener('scroll', handleScroll, { passive: true });
        }
        
        // 추가 도구 로드 (필요시)
        async function loadMoreTools() {
            if (!window.dbManager || !window.dbManager.isInitialized) return;
            
            try {
                // 전체 도구가 이미 로드되었는지 확인
                if (allTools.length > 0) {
                    console.log('모든 도구가 이미 로드되었습니다.');
                    return;
                }
                
                // 백그라운드에서 전체 도구 로드 시작
                await window.dbManager.loadAllToolsInBackground();
                
            } catch (error) {
                console.warn('추가 도구 로드 실패:', error);
            }
        }
        
        // 페이지 초기화 완료 후 지연 로딩 설정
        function initializeLazyLoading() {
            // 이미지 지연 로딩 설정
            setupImageLazyLoading();
            
            // 스크롤 기반 지연 로딩 설정
            setupScrollBasedLazyLoading();
            
            console.log('지연 로딩 시스템 초기화 완료');
        }
        
        // 전역 폼 제출 차단 (새로고침 방지)
        function setupGlobalFormProtection() {
            document.addEventListener('submit', function(event) {
                console.log('🔍 전역 폼 제출 이벤트 감지:', {
                    formId: event.target.id,
                    formAction: event.target.action,
                    formMethod: event.target.method,
                    formClass: event.target.className
                });
                
                // 폼이 명시적으로 허용된 경우가 아니면 차단
                if (!event.target.hasAttribute('data-allow-submit')) {
                    event.preventDefault();
                    event.stopPropagation();
                    console.log('⚠️ 알 수 없는 폼 제출 차단됨 - 새로고침 방지');
                    return false;
                }
            });
            
            console.log('전역 폼 제출 보호 설정 완료');
        }
        
        // 성능 모니터링
        function setupPerformanceMonitoring() {
            // 페이지 로드 시간 측정
            window.addEventListener('load', () => {
                const loadTime = performance.now();
                console.log(`페이지 로드 완료: ${loadTime.toFixed(2)}ms`);
                
                // Core Web Vitals 측정
                if ('PerformanceObserver' in window) {
                    try {
                        const observer = new PerformanceObserver((list) => {
                            for (const entry of list.getEntries()) {
                                if (entry.name === 'first-contentful-paint') {
                                    console.log(`FCP: ${entry.startTime.toFixed(2)}ms`);
                                }
                                if (entry.name === 'largest-contentful-paint') {
                                    console.log(`LCP: ${entry.startTime.toFixed(2)}ms`);
                                }
                            }
                        });
                        observer.observe({ entryTypes: ['paint', 'largest-contentful-paint'] });
                    } catch (e) {
                        console.warn('성능 측정 실패:', e);
                    }
                }
            });
        }
        
        // 로딩 상태 최적화
        function optimizeLoadingStates() {
            // 초기 로딩 상태 설정
            const loadingStates = {
                popularTools: false,
                allTools: false,
                images: false
            };
            
            // 로딩 상태 업데이트 함수
            function updateLoadingState(type, isLoading) {
                loadingStates[type] = isLoading;
                
                // 전체 로딩 상태 확인
                const allLoaded = Object.values(loadingStates).every(state => !state);
                
                if (allLoaded) {
                    hideAllLoadingIndicators();
                }
            }
            
            // 모든 로딩 인디케이터 숨기기
            function hideAllLoadingIndicators() {
                const loadingElements = document.querySelectorAll('.loading-indicator, .animate-pulse');
                loadingElements.forEach(el => {
                    el.style.display = 'none';
                });
                
                console.log('모든 로딩 완료');
            }
            
            // 전역 함수로 노출
            window.updateLoadingState = updateLoadingState;
        }
        
        // 메모리 사용량 최적화
        function optimizeMemoryUsage() {
            // 사용하지 않는 이벤트 리스너 정리
            function cleanupUnusedListeners() {
                // 스크롤 이벤트 디바운싱
                let scrollTimeout;
                const originalScrollHandler = window.onscroll;
                
                window.onscroll = function() {
                    if (scrollTimeout) {
                        clearTimeout(scrollTimeout);
                    }
                    scrollTimeout = setTimeout(() => {
                        if (originalScrollHandler) {
                            originalScrollHandler.call(this);
                        }
                    }, 100);
                };
            }
            
            // 이미지 캐시 최적화
            function optimizeImageCache() {
                const images = document.querySelectorAll('img');
                images.forEach(img => {
                    // 이미지 로드 완료 후 메모리 정리
                    img.addEventListener('load', () => {
                        img.classList.add('loaded');
                    });
                    
                    // 에러 발생 시 기본 이미지로 대체
                    img.addEventListener('error', () => {
                        img.src = window.DEFAULT_IMAGE;
                    });
                });
            }
            
            cleanupUnusedListeners();
            optimizeImageCache();
        }
        
        // 초기화 함수들 실행
        document.addEventListener('DOMContentLoaded', () => {
            setupPerformanceMonitoring();
            optimizeLoadingStates();
            optimizeMemoryUsage();
            setupGlobalFormProtection(); // 전역 폼 제출 보호 추가
            
            // 지연 로딩 시스템 초기화
            setTimeout(initializeLazyLoading, 1000);
        });
    </script>

