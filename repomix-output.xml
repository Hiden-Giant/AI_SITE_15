        if (dbManagerReadyHandled) return;
        dbManagerReadyHandled = true;
        
        console.log('DBManager 준비 완료:', event.detail);
        
        if (event.detail && event.detail.success) {
            hideLoading();
        } else {
            showError(event.detail?.error || '데이터베이스 초기화 실패');
            hideLoading();
        }
