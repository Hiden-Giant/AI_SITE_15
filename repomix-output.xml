}

.theme-toggle i {
    font-size: 1.25rem;
}

/* 태그 컴포넌트 */
.tag {
    background: var(--hover-bg);
    color: var(--text-secondary);
    padding: 0.25rem 0.75rem;
    border-radius: 9999px;
    font-size: 0.875rem;
    display: inline-block;
    margin-right: 0.25rem;
    margin-bottom: 0.25rem;
