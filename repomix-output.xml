                let description = categoryNames.length === 1
                    ? `${categoryNames[0]} ê°€ëŠ¥í•œ`
                    : `${categoryNames.join(' ë° ')} ëª¨ë‘ ê°€ëŠ¥í•œ`;
                
                descriptionElement.textContent = description;
            } else {
                descriptionElement.textContent = 'ì „ì²´';
            }
        }

        // ì •ë ¬ í•¨ìˆ˜ ì¶”ê°€
        function sortTools(tools, sortKey) {
            const toolsArr = Object.entries(tools);
            switch(sortKey) {
                case 'rating':
                    toolsArr.sort((a, b) => (b[1].rating || 0) - (a[1].rating || 0));
                    break;
                case 'newest':
                    toolsArr.sort((a, b) => (b[1].year || 0) - (a[1].year || 0));
                    break;
                case 'popular':
                    toolsArr.sort((a, b) => (b[1].popularity || 0) - (a[1].popularity || 0));
                    break;
                default:
                    break;
            }
            return Object.fromEntries(toolsArr);
        }

        // ì´ë¯¸ì§€ URL ìœ íš¨ì„± ì²´í¬ í•¨ìˆ˜ (ê°€ì¥ ìœ„ì— ì„ ì–¸)
        // ì´ í•¨ìˆ˜ëŠ” ì´ì œ windowì— í• ë‹¹ë˜ë¯€ë¡œ í…œí”Œë¦¿ì—ì„œ ì§ì ‘ í˜¸ì¶œí•  ìˆ˜ ìˆìŠµë‹ˆë‹¤.
    </script>
