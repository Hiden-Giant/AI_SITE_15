                }
            } catch (error) {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(error, 'í˜ì´ì§€ ì´ˆê¸°í™”');
                } else {
                    console.error('í˜ì´ì§€ ì´ˆê¸°í™” ì˜¤ë¥˜:', error);
