                        }
                        
                        // ì¦ê²¨ì°¾ê¸° ë„êµ¬/ë ˆì‹œí”¼ ë¡œë“œ í•¨ìˆ˜ í˜¸ì¶œ
                        loadFavoriteTools(user);
                        loadFavoriteRecipes(user);
                        
                        // ë³€ê²½ì‚¬í•­ ì €ì¥ ë²„íŠ¼ ì´ë²¤íŠ¸ ë¦¬ìŠ¤ë„ˆ
                        setupSaveChangesListener(user);
