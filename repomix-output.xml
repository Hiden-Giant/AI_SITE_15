.card-component.compact {
    padding: 1rem;
}

.card-component.large {
    padding: 2rem;
}

/* === 버튼 컴포넌트 === */
.btn-component {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 0.5rem;
    padding: 0.75rem 1.5rem;
    border-radius: 0.5rem;
    font-weight: 600;
    font-size: 1rem;
    text-decoration: none;
    border: none;
    cursor: pointer;
    transition: all 0.3s ease;
    white-space: nowrap;
}

.btn-component.primary {
    background: linear-gradient(90deg, var(--gradient-start), var(--gradient-end));
    color: white;
}

.btn-component.primary:hover {
    opacity: 0.9;
    transform: none;
}

.btn-component.secondary {
    background: var(--button-secondary);
    color: white;
}

.btn-component.secondary:hover {
    background: var(--button-secondary-hover);
    transform: translateY(-1px);
}

.btn-component.outline {
    background: transparent;
    color: var(--text-primary);
    border: 2px solid var(--border-color);
}

.btn-component.outline:hover {
    background: var(--hover-bg);
    border-color: var(--border-hover);
}

.btn-component.small {
    padding: 0.5rem 1rem;
    font-size: 0.875rem;
}

.btn-component.large {
    padding: 1rem 2rem;
    font-size: 1.125rem;
}

.btn-component:disabled {
    opacity: 0.5;
    cursor: not-allowed;
    transform: none !important;
}

/* === 태그 컴포넌트 === */
.tag-component {
    display: inline-flex;
    align-items: center;
    gap: 0.25rem;
    padding: 0.25rem 0.75rem;
    border-radius: 9999px;
    font-size: 0.875rem;
    font-weight: 500;
    background: var(--hover-bg);
    color: var(--text-secondary);
    border: 1px solid transparent;
    transition: all 0.3s ease;
}

.tag-component.primary {
    background: var(--gradient-start);
    color: white;
}

.tag-component.success {
    background: var(--success-color);
    color: white;
}

.tag-component.warning {
    background: var(--warning-color);
    color: white;
}

.tag-component.error {
    background: var(--error-color);
    color: white;
}

.tag-component.info {
    background: var(--info-color);
    color: white;
}

.tag-component.removable {
    padding-right: 0.5rem;
}

.tag-component.removable .remove-btn {
    background: none;
    border: none;
    color: inherit;
    cursor: pointer;
    padding: 0.125rem;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    transition: background-color 0.2s ease;
}

.tag-component.removable .remove-btn:hover {
    background: rgba(255, 255, 255, 0.2);
}

/* === 입력 필드 컴포넌트 === */
.input-component {
    width: 100%;
    padding: 0.75rem;
    border: 1px solid var(--border-color);
    border-radius: 0.5rem;
    background: var(--input-bg);
    color: var(--text-primary);
    font-size: 1rem;
    transition: all 0.3s ease;
}

.input-component:focus {
    outline: none;
    border-color: var(--gradient-start);
    box-shadow: 0 0 0 3px rgba(124, 58, 237, 0.1);
}

.input-component.error {
    border-color: var(--error-color);
    box-shadow: 0 0 0 3px rgba(239, 68, 68, 0.1);
}

.input-component.success {
    border-color: var(--success-color);
    box-shadow: 0 0 0 3px rgba(16, 185, 129, 0.1);
}

.input-component.warning {
    border-color: var(--warning-color);
    box-shadow: 0 0 0 3px rgba(245, 158, 11, 0.1);
}

.input-component.large {
    padding: 1rem;
    font-size: 1.125rem;
}

.input-component.small {
    padding: 0.5rem;
    font-size: 0.875rem;
}

/* === 모달 컴포넌트 === */
.modal-overlay {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: var(--modal-bg);
    z-index: 1000;
    justify-content: center;
    align-items: center;
    backdrop-filter: blur(4px);
}

.modal-overlay.show {
    display: flex;
    animation: fadeIn 0.3s ease-out;
}

.modal-component {
    background: var(--card-bg);
    border-radius: 1rem;
    padding: 2rem;
    max-width: 500px;
    width: 90%;
    max-height: 80vh;
    overflow-y: auto;
    position: relative;
    box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.3);
    border: 1px solid var(--border-color);
}

.modal-component.large {
    max-width: 800px;
}

.modal-component.small {
    max-width: 400px;
    padding: 1.5rem;
}

.modal-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 1.5rem;
    padding-bottom: 1rem;
    border-bottom: 1px solid var(--border-color);
}

.modal-title {
    font-size: 1.5rem;
    font-weight: 600;
    color: var(--text-primary);
    margin: 0;
}

.modal-close {
    background: none;
    border: none;
    font-size: 1.5rem;
    color: var(--text-secondary);
    cursor: pointer;
    padding: 0.5rem;
    border-radius: 0.25rem;
    transition: all 0.2s ease;
}

.modal-close:hover {
    background: var(--hover-bg);
    color: var(--text-primary);
}

.modal-body {
    margin-bottom: 1.5rem;
}

.modal-footer {
    display: flex;
    justify-content: flex-end;
    gap: 1rem;
    padding-top: 1rem;
    border-top: 1px solid var(--border-color);
}

/* === 알림 컴포넌트 === */
.alert-component {
    display: flex;
    align-items: flex-start;
    gap: 0.75rem;
    padding: 1rem;
    border-radius: 0.5rem;
    margin: 1rem 0;
    border: 1px solid transparent;
    animation: slideInDown 0.3s ease-out;
}

.alert-component.info {
    background: rgba(6, 182, 212, 0.1);
    border-color: var(--info-color);
    color: var(--info-color);
}

.alert-component.success {
    background: rgba(16, 185, 129, 0.1);
    border-color: var(--success-color);
    color: var(--success-color);
}

.alert-component.warning {
    background: rgba(245, 158, 11, 0.1);
    border-color: var(--warning-color);
    color: var(--warning-color);
}

.alert-component.error {
    background: rgba(239, 68, 68, 0.1);
    border-color: var(--error-color);
    color: var(--error-color);
}

.alert-icon {
    flex-shrink: 0;
    font-size: 1.25rem;
}

.alert-content {
    flex: 1;
}

.alert-title {
    font-weight: 600;
    margin-bottom: 0.25rem;
}

.alert-message {
    margin: 0;
    line-height: 1.5;
}

.alert-close {
    background: none;
    border: none;
    color: inherit;
    cursor: pointer;
    padding: 0.25rem;
    border-radius: 0.25rem;
    transition: background-color 0.2s ease;
}

.alert-close:hover {
    background: rgba(255, 255, 255, 0.1);
}

/* === 로딩 컴포넌트 === */
.loading-component {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 2rem;
    text-align: center;
}

.loading-spinner {
    width: 3rem;
    height: 3rem;
    border: 3px solid var(--border-color);
    border-top: 3px solid var(--gradient-start);
    border-radius: 50%;
    animation: spin 1s linear infinite;
    margin-bottom: 1rem;
}

.loading-text {
    color: var(--text-secondary);
    font-size: 1rem;
    margin: 0;
}

.loading-component.small .loading-spinner {
    width: 2rem;
    height: 2rem;
    border-width: 2px;
}

.loading-component.small .loading-text {
    font-size: 0.875rem;
}

.loading-component.large .loading-spinner {
    width: 4rem;
    height: 4rem;
    border-width: 4px;
}

.loading-component.large .loading-text {
    font-size: 1.125rem;
}

/* === 네비게이션 컴포넌트 === */
.nav-component {
    display: flex;
    align-items: center;
    gap: 1rem;
    padding: 1rem 0;
}

.nav-item {
    padding: 0.5rem 1rem;
    border-radius: 0.5rem;
    color: var(--text-secondary);
    text-decoration: none;
    transition: all 0.3s ease;
    font-weight: 500;
}

.nav-item:hover {
    color: var(--text-primary);
    background: var(--hover-bg);
}

.nav-item.active {
    color: var(--gradient-start);
    background: rgba(124, 58, 237, 0.1);
}

.nav-item.disabled {
    opacity: 0.5;
    cursor: not-allowed;
    pointer-events: none;
}

/* === 페이지네이션 컴포넌트 === */
.pagination-component {
    display: flex;
    align-items: center;
    gap: 0.5rem;
    justify-content: center;
    margin: 2rem 0;
}

.pagination-item {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 2.5rem;
    height: 2.5rem;
    border-radius: 0.5rem;
    color: var(--text-secondary);
    text-decoration: none;
    transition: all 0.3s ease;
    font-weight: 500;
    border: 1px solid transparent;
}

.pagination-item:hover {
    color: var(--text-primary);
    background: var(--hover-bg);
    border-color: var(--border-color);
}

.pagination-item.active {
    color: white;
    background: var(--gradient-start);
    border-color: var(--gradient-start);
}

.pagination-item.disabled {
    opacity: 0.5;
    cursor: not-allowed;
    pointer-events: none;
}

/* === 애니메이션 === */
@keyframes fadeIn {
    from {
        opacity: 0;
        transform: none;
    }
    to {
        opacity: 1;
        transform: none;
    }
}

@keyframes slideInDown {
    from {
        transform: none;
        opacity: 0;
    }
    to {
        transform: none;
        opacity: 1;
    }
}

@keyframes slideInRight {
    from {
        transform: none;
        opacity: 0;
    }
    to {
        transform: none;
        opacity: 1;
    }
}

@keyframes spin {
    from {
        transform: none;
    }
    to {
        transform: none;
    }
}

/* === 반응형 디자인 === */
@media (max-width: 768px) {
    .modal-component {
        width: 95%;
        margin: 1rem;
        padding: 1.5rem;
    }
    
    .card-component {
        padding: 1rem;
    }
    
    .btn-component {
        padding: 0.625rem 1.25rem;
        font-size: 0.875rem;
    }
    
    .nav-component {
        flex-direction: column;
        gap: 0.5rem;
    }
    
    .pagination-component {
        gap: 0.25rem;
    }
    
    .pagination-item {
        width: 2rem;
        height: 2rem;
        font-size: 0.875rem;
    }
}

@media (max-width: 640px) {
    .modal-component {
        width: 100%;
        margin: 0.5rem;
        border-radius: 0.5rem;
    }
    
    .alert-component {
        padding: 0.75rem;
        margin: 0.5rem 0;
    }
    
    .loading-component {
        padding: 1rem;
    }
}

/* === 프로필 페이지 전용 컴포넌트 === */
.profile-stats {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 1rem;
    margin-top: 1rem;
}

.stat-item {
    text-align: center;
    padding: 1rem;
    background-color: var(--card-bg);
    border-radius: 0.5rem;
    border: 1px solid var(--border-color);
    transition: all 0.3s ease;
}

.stat-item:hover {
    transform: translateY(-2px);
    border-color: var(--highlight);
}

/* 즐겨찾기 탭 스타일 */
.favorite-tabs {
    display: flex;
    space-x: 1;
    margin-bottom: 1.5rem;
    background: var(--card-bg);
    border-radius: 0.5rem;
    padding: 0.25rem;
}

.favorite-tab {
    flex: 1;
    padding: 0.5rem 1rem;
    border-radius: 0.375rem;
    font-weight: 500;
    transition: all 0.3s ease;
    cursor: pointer;
    border: none;
    background: transparent;
    color: var(--text-secondary);
}

.favorite-tab.active {
    background: var(--highlight);
    color: white;
}

.favorite-tab:hover:not(.active) {
    background: var(--hover-bg);
    color: var(--text-primary);
}

/* 레시피 카드 스타일 */
.recipe-card {
    background: var(--card-bg);
    color: var(--text-primary);
    border-radius: 0.75rem;
    box-shadow: var(--card-shadow);
    transition: all 0.3s ease;
    border: 1px solid var(--border-color);
    cursor: pointer;
    position: relative;
    overflow: hidden;
    word-wrap: break-word;
    height: 380px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.recipe-card:hover {
    transform: translateY(-4px);
    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.15);
    background: var(--card-hover);
    border-color: var(--highlight);
}

.recipe-tool-icon {
    width: 36px;
    height: 36px;
    border-radius: 0.5rem;
    display: flex;
    align-items: center;
    justify-content: center;
    color: white;
    font-size: 14px;
    overflow: hidden;
    flex-shrink: 0;
    position: relative;
}

.recipe-tool-container {
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
    min-width: 0;
    flex-shrink: 0;
    overflow: hidden;
}

.recipe-tool-name {
    font-size: 8px;
    color: var(--text-secondary);
    margin-top: 3px;
    max-width: 45px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: center;
}

.recipe-arrow {
    font-size: 12px;
    color: var(--text-muted);
    flex-shrink: 0;
}

/* 태그 영역 2줄 제한 */
.tag-list-clamp {
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    max-height: 3.8em;
}

/* 로딩 상태 표시 */
.loading-skeleton {
    background: linear-gradient(90deg, var(--hover-bg) 25%, var(--border-color) 50%, var(--hover-bg) 75%);
    background-size: 200% 100%;
    animation: loading 1.5s infinite;
}

@keyframes loading {
    0% { background-position: 200% 0; }
    100% { background-position: -200% 0; }
}

/* 모바일 최적화 */
@media (max-width: 768px) {
    .profile-stats {
        grid-template-columns: repeat(3, 1fr);
        gap: 0.5rem;
    }
    
    .stat-item {
        padding: 0.75rem 0.5rem;
    }
    
    .recipe-card {
        height: auto;
        min-height: 320px;
    }
    
    .recipe-tool-icon {
        width: 32px;
        height: 32px;
        font-size: 12px;
    }
    
    .recipe-tool-name {
        font-size: 7px;
        max-width: 40px;
    }
    
    .favorite-tabs {
        flex-direction: column;
        space-x: 0;
        space-y: 0.25rem;
    }
    
    .favorite-tab {
        border-radius: 0.375rem;
    }
}
