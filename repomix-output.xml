        // 레시피 상세 정보 로드
        async function loadRecipeDetail(recipeId) {
            try {
                const recipeRef = collection(db, 'public_recipe_collection');
                const q = query(recipeRef, where('__name__', '==', recipeId));
                const querySnapshot = await getDocs(q);
                
