    </script>

    
    <!-- 다국어 관련 스크립트는 header.html에서 이미 로드됨 -->

    <style>
        /* 이 페이지에서만 사용되는 고유한 스타일 */
        
        /* 헤더 컨테이너 너비 조정 */
