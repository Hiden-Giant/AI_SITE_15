    padding: 0.75rem;
    border: 1px solid var(--border-color);
    border-radius: 0.5rem;
    background: var(--input-bg);
    color: var(--text-primary);
    font-size: 1rem;
    transition: border-color 0.3s ease;
}

.input-field:focus {
    outline: none;
    border-color: var(--gradient-start);
    box-shadow: 0 0 0 3px rgba(124, 58, 237, 0.1);
}

/* 스크린 리더 전용 */
.sr-only {
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip: rect(0, 0, 0, 0);
    white-space: nowrap;
    border: 0;
}

/* 반응형 디자인 */
@media (max-width: 768px) {
    .modal {
        width: 95%;
        padding: 1.5rem;
    }
    
    .card {
        padding: 1rem;
    }
    
    .btn-primary,
    .btn-secondary {
        padding: 0.625rem 1.25rem;
        font-size: 0.875rem;
    }
}

@media (max-width: 640px) {
    .modal {
        width: 100%;
        margin: 1rem;
        border-radius: 0.5rem;
    }
    
    .error-message,
    .success-message,
    .warning-message {
        margin: 0.5rem 0;
        padding: 0.75rem;
    }
}

/* 애니메이션 */
.fade-in {
    animation: fadeIn 0.5s ease-in;
}

@keyframes fadeIn {
    from {
        opacity: 0;
        transform: none;
    }
    to {
        opacity: 1;
        transform: none;
    }
}

/* 테마 전환 애니메이션 */
.theme-transition {
    transition: background-color 0.3s ease, color 0.3s ease, border-color 0.3s ease, box-shadow 0.3s ease;
}

/* 입력 필드 상태 스타일 */
.input-field.error {
    border-color: var(--error-color);
}

.input-field.error:focus {
    box-shadow: 0 0 0 3px rgba(239, 68, 68, 0.1);
}

.field-error-message {
    display: none;
    color: var(--error-color);
    font-size: 0.875rem;
    margin-top: 0.25rem;
    padding: 0.5rem;
    background: rgba(239, 68, 68, 0.1);
    border-radius: 0.25rem;
}

.field-error-message.show {
    display: block;
    animation: slideInRight 0.3s ease-out;
}

@keyframes slideInRight {
    from {
        transform: none;
        opacity: 0;
    }
    to {
        transform: none;
        opacity: 1;
    }
}

/* 에러 아이콘 */
.error-icon {
    color: var(--error-color);
    margin-right: 0.5rem;
}

.input-field.success {
    border-color: var(--success-color);
}

.input-field.warning {
    border-color: var(--warning-color);
}

/* 에러 요약 */
.error-summary {
    background: rgba(239, 68, 68, 0.1);
    border: 1px solid var(--error-color);
    border-radius: 0.5rem;
    padding: 1rem;
    margin: 1rem 0;
}

.error-summary h3 {
    color: var(--error-color);
    margin-bottom: 0.5rem;
    font-size: 1.125rem;
}

.error-summary ul {
    margin: 0;
    padding-left: 1.5rem;
}

.error-summary li {
    color: var(--text-secondary);
    margin-bottom: 0.25rem;
}

/* 에러 복구 버튼 */
.error-recovery {
    background: var(--error-color);
    color: white;
    padding: 0.5rem 1rem;
    border: none;
    border-radius: 0.25rem;
    cursor: pointer;
    font-size: 0.875rem;
    transition: background-color 0.3s ease;
}

.error-recovery:hover {
    background: #dc2626;
}

/* 에러 통계 */
.error-stats {
    display: none;
    background: var(--card-bg);
    border: 1px solid var(--border-color);
    border-radius: 0.5rem;
    padding: 1rem;
    margin-top: 1rem;
}

.error-stats.show {
    display: block;
}

.error-stats h4 {
    color: var(--text-primary);
    margin-bottom: 0.5rem;
    font-size: 1rem;
}

.error-stats table {
    width: 100%;
    border-collapse: collapse;
}

.error-stats th,
.error-stats td {
    padding: 0.5rem;
    text-align: left;
    border-bottom: 1px solid var(--border-color);
}

/* 모바일 최적화 */
@media (max-width: 768px) {
    .error-summary {
        padding: 0.75rem;
        margin: 0.5rem 0;
    }
    
    .field-error-message {
        font-size: 0.8rem;
        padding: 0.375rem;
    }
    
    .error-stats {
        padding: 0.75rem;
        font-size: 0.875rem;
    }
}
</file>

