    <!-- JavaScript -->
    <script>
        // 모바일 메뉴 토글
        document.addEventListener('DOMContentLoaded', function() {
            console.log('=== DOMContentLoaded 이벤트 발생 ===');
            
            // 모달 외부 클릭 설정
            setupModalOutsideClick();
            
                    // 폼 제출 이벤트 리스너 추가 (공통 모듈 사용)
        const loginForm = document.getElementById('loginForm');
        const signupForm = document.getElementById('signupForm');
        
        if (loginForm) {
            console.log('로그인 폼 이벤트 리스너 추가');
            
            // 폼의 action 속성 제거 및 novalidate 설정
            loginForm.removeAttribute('action');
            loginForm.setAttribute('novalidate', 'true');
            
        } else {
            console.error('로그인 폼을 찾을 수 없습니다!');
        }
        
        if (signupForm) {
            console.log('회원가입 폼 이벤트 리스너 추가');
            
            // 폼의 action 속성 제거 및 novalidate 설정
            signupForm.removeAttribute('action');
            signupForm.setAttribute('novalidate', 'true');
            
        } else {
            console.error('회원가입 폼을 찾을 수 없습니다!');
        }
            
            // 추가 보안: 모든 폼 제출 차단
            document.addEventListener('submit', function(event) {
                console.log('전역 폼 제출 이벤트 감지:', event.target.id);
                
                // 로그인/회원가입 폼이 아닌 경우에만 기본 동작 차단
                if (event.target.id !== 'loginForm' && event.target.id !== 'signupForm') {
                    event.preventDefault();
                    event.stopPropagation();
                    console.log('알 수 없는 폼 제출 차단됨');
                    return false;
                }
            });
            
            // 추가 보안: 모든 링크 클릭 이벤트 감지
            document.addEventListener('click', function(event) {
                if (event.target.tagName === 'A' && event.target.href === 'javascript:void(0)') {
                    console.log('javascript:void(0) 링크 클릭 감지');
                }
            });
            
            // 저장된 인증 상태 확인 및 복원
            const savedAuthUser = localStorage.getItem('authUser');
            if (savedAuthUser) {
                try {
                    const user = JSON.parse(savedAuthUser);
                    console.log('localStorage에서 저장된 사용자 정보 복원:', user);
                    
                    // AuthManager가 준비될 때까지 대기
                    if (window.authManager && window.authManager.isInitialized) {
                        updateHeaderAuthUI(user);
                    } else {
                        window.addEventListener('authManagerReady', () => {
                            updateHeaderAuthUI(user);
                        }, { once: true });
                    }
                } catch (error) {
                    console.error('저장된 인증 정보 파싱 실패:', error);
                    localStorage.removeItem('authUser');
                }
            }
            
            // 인증 상태 확인 및 UI 업데이트
            if (window.auth && window.auth.currentUser) {
                updateHeaderAuthUI(window.auth.currentUser);
            }
            
            const hamburgerBtn = document.getElementById('hamburger-btn');
            const navMobile = document.getElementById('nav-mobile');
            let isOpen = false;

            if (hamburgerBtn && navMobile) {
                hamburgerBtn.addEventListener('click', function(e) {
                    e.stopPropagation();
                    if (isOpen) {
                        navMobile.classList.remove('active');
                        hamburgerBtn.querySelector('i').className = 'fas fa-bars';
                        isOpen = false;
                    } else {
                        navMobile.classList.add('active');
                        hamburgerBtn.querySelector('i').className = 'fas fa-times';
                        isOpen = true;
                    }
                });

                // 외부 클릭 시 메뉴 닫기
                document.addEventListener('click', function(e) {
                    if (isOpen && !navMobile.contains(e.target) && !hamburgerBtn.contains(e.target)) {
                        navMobile.classList.remove('active');
                        hamburgerBtn.querySelector('i').className = 'fas fa-bars';
                        isOpen = false;
                    }
                });
            }

            // 프로필 드롭다운 토글
            const profileMenuBtn = document.getElementById('profileMenuButton');
            const profileDropdown = document.getElementById('profileDropdown');
            
            if (profileMenuBtn && profileDropdown) {
                profileMenuBtn.addEventListener('click', function(e) {
                    e.stopPropagation();
                    if (profileDropdown.style.display === 'block') {
                        profileDropdown.style.display = 'none';
                    } else {
                        profileDropdown.style.display = 'block';
                    }
                });

                // 외부 클릭 시 드롭다운 닫기
                document.addEventListener('click', function(e) {
                    if (!profileMenuBtn.contains(e.target) && !profileDropdown.contains(e.target)) {
                        profileDropdown.style.display = 'none';
                    }
                });
            }

            // 언어 선택기
            const languageSwitcher = document.getElementById('languageSwitcher');
            const mobileLanguageSwitcher = document.getElementById('mobileLanguageSwitcher');
            
            if (languageSwitcher) {
                languageSwitcher.addEventListener('change', function() {
                    const selectedLang = this.value;
                    if (window.changeLanguage) {
                        window.changeLanguage(selectedLang);
                    }
                    // 모바일 언어 선택기도 동기화
                    if (mobileLanguageSwitcher) {
                        mobileLanguageSwitcher.value = selectedLang;
                    }
                });
            }
            
            if (mobileLanguageSwitcher) {
                mobileLanguageSwitcher.addEventListener('change', function() {
                    const selectedLang = this.value;
                    if (window.changeLanguage) {
                        window.changeLanguage(selectedLang);
                    }
                    // 데스크톱 언어 선택기도 동기화
                    if (languageSwitcher) {
                        languageSwitcher.value = selectedLang;
                    }
                });
            }
        });

        // 네비게이션 함수들
        function navigateToProfile() {
            window.location.href = 'profile.html';
        }

        function navigateToSettings() {
            // 설정 페이지로 이동하는 로직
            console.log('설정 페이지로 이동');
        }

        async function handleLogout() {
            try {
                if (window.authManager && window.authManager.handleLogout) {
                    await window.authManager.handleLogout();
                    window.location.reload();
                    return;
                }
                // 하위 호환: v10 모듈 API 동적 import 시도
                const { getAuth, signOut } = await import('https://www.gstatic.com/firebasejs/10.8.0/firebase-auth.js');
                const auth = window.auth || (window.app ? getAuth(window.app) : getAuth());
                await signOut(auth);
                window.location.reload();
            } catch (error) {
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(error, '로그아웃');
                } else {
                    console.error('로그아웃 오류:', error);
                    alert('로그아웃 중 오류가 발생했습니다.');
                }
            }
        }

        // 모달 표시 함수들



        
        // 모달 닫기 함수들

        

        
        // 모달 외부 클릭 시 닫기 (공통 모듈 사용)
        function setupModalOutsideClick() {
            const loginModal = document.getElementById('loginModal');
            const signupModal = document.getElementById('signupModal');
            
            if (loginModal) {
                loginModal.addEventListener('click', (e) => {
                    if (e.target === loginModal) {
                        window.closeLoginModal();
                    }
                });
            }
            
            if (signupModal) {
                signupModal.addEventListener('click', (e) => {
                    if (e.target === signupModal) {
                        window.closeSignupModal();
                    }
                });
            }
        }




        

        
        // 전역 함수로 등록 (공통 모듈 사용)
        window.navigateToProfile = navigateToProfile;
        window.navigateToSettings = navigateToSettings;
        window.handleLogout = handleLogout;
        
        // 구글 소셜 로그인 함수
        async function handleSocialLogin(provider) {
            try {
                // AuthManager 초기화 대기
                if (!window.authManager || !window.authManager.isInitialized) {
                    await new Promise((resolve) => {
                        window.addEventListener('authManagerReady', resolve, { once: true });
                    });
                }
                
                if (window.authManager && window.authManager.handleSocialLogin) {
                    await window.authManager.handleSocialLogin(provider);
                } else {
                    throw new Error('AuthManager가 초기화되지 않았습니다.');
                }
            } catch (error) {
                console.error('소셜 로그인 실패:', error);
                alert('소셜 로그인에 실패했습니다. 다시 시도해주세요.');
            }
        }
        
        window.handleSocialLogin = handleSocialLogin;
        
        // 테스트용 함수들
        function testFunction() {
            console.log('=== testFunction 호출됨 ===');
            alert('테스트 함수가 정상적으로 호출되었습니다!');
        }
        
        function testAuthManager() {
            console.log('=== testAuthManager 호출됨 ===');
            console.log('AuthManager 상태:', {
                authManager: !!window.authManager,
                isInitialized: window.authManager?.isInitialized,
                auth: !!window.auth,
                app: !!window.app
            });
        }
        

        
        // 추가 디버깅 함수들
        function debugLoginForm() {
            console.log('=== 로그인 폼 디버깅 ===');
            const loginForm = document.getElementById('loginForm');
            const emailInput = document.getElementById('loginEmail');
            const passwordInput = document.getElementById('loginPassword');
            
            console.log('폼 요소들:', {
                loginForm: !!loginForm,
                emailInput: !!emailInput,
                passwordInput: !!passwordInput
            });
            
            if (loginForm) {
                console.log('로그인 폼 속성:', {
                    id: loginForm.id,
                    action: loginForm.action,
                    method: loginForm.method,
                    onsubmit: loginForm.onsubmit
                });
            }
            
            if (emailInput) {
                console.log('이메일 입력값:', {
                    value: emailInput.value,
                    type: emailInput.type,
                    required: emailInput.required
                });
            }
            
            if (passwordInput) {
                console.log('비밀번호 입력값:', {
                    value: passwordInput.value ? '***' : '(비어있음)',
                    type: passwordInput.type,
                    required: passwordInput.required
                });
            }
        }
        
        function debugEventHandlers() {
            console.log('=== 이벤트 핸들러 디버깅 ===');
            const loginForm = document.getElementById('loginForm');
            
            if (loginForm) {
                // 이벤트 리스너 확인 (간접적 방법)
                console.log('폼 이벤트 핸들러:', {
                    onsubmit: loginForm.onsubmit,
                    hasAttribute: loginForm.hasAttribute('onsubmit')
                });
                
                // 전역 이벤트 리스너 확인
                console.log('전역 submit 이벤트 리스너 등록됨:', true);
            }
        }
        
        // 전역에 등록
        window.testFunction = testFunction;
        window.testAuthManager = testAuthManager;
        window.debugLoginForm = debugLoginForm;
        window.debugEventHandlers = debugEventHandlers;
        window.handleLoginFormSubmit = handleLoginFormSubmit;
        
        console.log('테스트 함수들이 전역에 등록되었습니다.');
        
        // 인증 상태에 따른 UI 업데이트 함수
        function updateHeaderAuthUI(user) {
            if (user) {
                // 로그인 상태: 프로필 메뉴 표시
                const authButtons = document.getElementById('authButtons');
                const profileMenu = document.getElementById('profileMenu');
                const mobileAuthButtons = document.getElementById('mobileAuthButtons');
                const mobileProfileMenu = document.getElementById('mobileProfileMenu');
                
                if (authButtons) authButtons.style.display = 'none';
                if (profileMenu) profileMenu.style.display = 'block';
                if (mobileAuthButtons) mobileAuthButtons.style.display = 'none';
                if (mobileProfileMenu) mobileProfileMenu.style.display = 'flex';
                
                // 사용자 정보 업데이트
                const profileInitial = document.getElementById('profileInitial');
                const profileName = document.getElementById('profileName');
                const profileEmail = document.getElementById('profileEmail');
                const profileMenuButton = document.getElementById('profileMenuButton');
                const mobileProfileName = document.getElementById('mobileProfileName');
                
                if (profileInitial) {
                    profileInitial.textContent = user.displayName ? user.displayName.charAt(0).toUpperCase() : user.email.charAt(0).toUpperCase();
                }
                if (profileName) {
                    profileName.textContent = user.displayName || '사용자';
                }
                if (profileEmail) {
                    profileEmail.textContent = user.email;
                }
                if (profileMenuButton) {
                    profileMenuButton.setAttribute('data-email', user.email);
                }
                if (mobileProfileName) {
                    mobileProfileName.textContent = user.displayName || '사용자';
                }
                
            } else {
                // 로그아웃 상태: 로그인 버튼 표시
                const authButtons = document.getElementById('authButtons');
                const profileMenu = document.getElementById('profileMenu');
                const mobileAuthButtons = document.getElementById('mobileAuthButtons');
                const mobileProfileMenu = document.getElementById('mobileProfileMenu');
                
                if (authButtons) authButtons.style.display = 'flex';
                if (profileMenu) profileMenu.style.display = 'none';
                if (mobileAuthButtons) mobileAuthButtons.style.display = 'flex';
                if (mobileProfileMenu) mobileProfileMenu.style.display = 'none';
            }
        }
        
        // 전역 함수로 등록
        window.updateHeaderAuthUI = updateHeaderAuthUI;
    </script>

    <!-- 외부 스크립트 파일들 -->
    <script src="js/header-menu.js"></script>
    <script src="js/theme-manager.js"></script>
    <!-- auth-manager.js 로드 추가 -->
    <script type="module" src="js/auth-manager.js"></script>

</body>
