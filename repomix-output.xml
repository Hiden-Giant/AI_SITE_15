                        } else {
                            selectedCategories.add(category);
                            this.classList.add('selected');
                        }
                        filterAndDisplayTools();
                    });
                });

                // ì •ë ¬ ì˜µì…˜ ë³€ê²½ ì´ë²¤íŠ¸
                document.getElementById('sort-option').addEventListener('change', filterAndDisplayTools);
                
            } catch (error) {
                console.error("í˜ì´ì§€ ì´ˆê¸°í™” ì‹¤íŒ¨:", error);
                const container = document.getElementById('tools-container');
                if (container) {
                    handleError(container, error);
                }
            }
        });

