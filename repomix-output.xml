                const url = window.location.href;
                navigator.clipboard.writeText(url)
                    .then(() => alert('URL이 클립보드에 복사되었습니다.'))
                    .catch(err => console.log('클립보드 복사 실패:', err));
            }
        }

        // 저장하기 기능 토글
        function toggleSave(toolId) {
            const saveBtn = document.querySelector(`#save-btn-${toolId}`);
            const isSaved = saveBtn.classList.contains('saved');
            
            if (isSaved) {
                saveBtn.classList.remove('saved');
                saveBtn.innerHTML = '<i class="far fa-bookmark"></i> 저장하기';
            } else {
                saveBtn.classList.add('saved');
                saveBtn.innerHTML = '<i class="fas fa-bookmark"></i> 저장됨';
            }
        }

        // 페이지 로드 시 실행
        document.addEventListener('DOMContentLoaded', () => {
            // URL 파라미터에서 카드 ID 확인
            const urlParams = new URLSearchParams(window.location.search);
            const cardId = urlParams.get('card');
            
            // 카드 ID가 있으면 해당 카드의 상세 정보 표시
            if (cardId) {
                toggleDetails(cardId);
            }
        });
