        integratedBtn.className = 'flex-1 py-3 px-4 rounded-lg font-medium transition-all duration-300 bg-gray-300 dark:bg-gray-600 text-gray-700 dark:text-gray-300 hover:bg-gray-400 dark:hover:bg-gray-500';
