        // 변경사항 저장 리스너 설정
        function setupSaveChangesListener(user) {
            const profileForm = document.getElementById('profileForm');
            const saveChangesBtn = document.getElementById('saveChanges');
            
            if (profileForm) {
                profileForm.addEventListener('submit', async (e) => {
                    e.preventDefault(); // 폼 기본 제출 동작 방지
                    
                    // 버튼 비활성화 및 로딩 상태 표시
                    if (saveChangesBtn) {
                        saveChangesBtn.disabled = true;
                        saveChangesBtn.innerHTML = '<i class="fas fa-spinner fa-spin mr-2"></i>저장 중...';
                    }
                    
                    try {
                        const displayNameInput = document.getElementById('displayName');
                        const newDisplayName = displayNameInput ? displayNameInput.value.trim() : '';
                        
                        if (!newDisplayName) {
                            if (window.CommonUtils) {
                                window.CommonUtils.showError('이름을 입력해주세요.');
                            } else {
                                alert('이름을 입력해주세요.');
                            }
                            return;
                        }
                        
                        // Firebase Authentication 프로필 업데이트
                        await updateProfile(user, {
                            displayName: newDisplayName
                        });
                        
                        // Firestore 사용자 문서도 업데이트
                        await setDoc(doc(db, 'users', user.uid), { 
                            name: newDisplayName,
                            displayName: newDisplayName,
                            updatedAt: new Date()
                        }, { merge: true });
                        
                        // UI 업데이트
                        const profileNameDisplay = document.getElementById('profileNameDisplay');
                        const profileInitialLarge = document.getElementById('profileInitialLarge');
                        
                        if (profileNameDisplay) {
                            profileNameDisplay.textContent = newDisplayName;
                        }
                        if (profileInitialLarge) {
                            profileInitialLarge.textContent = newDisplayName[0].toUpperCase();
                        }
                        
                        // 헤더의 프로필 정보도 업데이트
                        const headerProfileName = document.getElementById('profileName');
                        const headerProfileInitial = document.getElementById('profileInitial');
                        if (headerProfileName) {
                            headerProfileName.textContent = newDisplayName;
                        }
                        if (headerProfileInitial) {
                            headerProfileInitial.textContent = newDisplayName[0].toUpperCase();
                        }
                        
                        if (window.CommonUtils) {
                            window.CommonUtils.showSuccess('프로필이 성공적으로 업데이트되었습니다.');
                        } else {
                            alert('프로필이 성공적으로 업데이트되었습니다.');
                        }
                        
                    } catch (error) {
                        console.error('프로필 업데이트 오류:', error);
                        
                        let errorMessage = '프로필 업데이트 중 오류가 발생했습니다.';
                        if (error.code === 'auth/requires-recent-login') {
                            errorMessage = '보안을 위해 다시 로그인한 후 시도해주세요.';
                        } else if (error.code === 'permission-denied') {
                            errorMessage = '프로필 업데이트 권한이 없습니다.';
                        } else if (error.code === 'unavailable') {
                            errorMessage = '네트워크 연결을 확인해주세요.';
                        }
                        
                        if (window.CommonUtils) {
                            window.CommonUtils.handleError(error, '프로필 업데이트');
                        } else {
                            alert(errorMessage);
                        }
                    } finally {
                        // 버튼 재활성화
                        if (saveChangesBtn) {
                            saveChangesBtn.disabled = false;
                            saveChangesBtn.innerHTML = '<i class="fas fa-save mr-2"></i>변경사항 저장';
                        }
                    }
                });
            }
        }

