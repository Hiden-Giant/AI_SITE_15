                        // íƒ­ ì´ë²¤íŠ¸ ë¦¬ìŠ¤ë„ˆ ì„¤ì • (ì•ˆì „í•˜ê²Œ)
                        const toolsTab = document.getElementById('favoriteToolsTab');
