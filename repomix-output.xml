        // 카테고리별 지연 로딩을 위한 변수
        let categoryLoadQueue = [];
        let isCategoryLoading = false;
        
        // Firebase 초기화 완료 이벤트 발생
        window.dispatchEvent(new CustomEvent('firebaseInitialized'));

        // AI 도구 정보 로드 함수
        async function loadToolImages() {
            try {
                console.log('AI 도구 정보 로드 시작...');
                
                if (!window.db) {
                    console.error('Firebase DB가 초기화되지 않았습니다.');
                    return;
                }
                
                const toolsRef = collection(window.db, 'ai-tools');
                console.log('ai-tools 컬렉션에 접근 중...');
                
                const toolsSnapshot = await getDocs(toolsRef);
                console.log('AI 도구 문서 수:', toolsSnapshot.size);
                
                toolsSnapshot.forEach((doc) => {
                    const data = doc.data();
                    if (data.name) {
                        // 이미지 URL 저장
                        if (data.imageUrl) {
                            toolImagesCache[data.name] = data.imageUrl;
                        }
                        
                        // 도구 정보 저장
                        toolInfoCache[data.name] = {
                            description: data.description || `${data.name}은 다양한 작업을 지원하는 AI 도구입니다.`,
                            category: data.category || '',
                            difficulty: data.difficulty || '',
                            websiteUrl: data.websiteUrl || '',
                            tags: data.tags || []
                        };
                    }
                });
                
                console.log(`${Object.keys(toolInfoCache).length}개의 AI 도구 정보를 로드했습니다.`);
                console.log('로드된 도구들:', Object.keys(toolInfoCache));
                
            } catch (error) {
                console.error('AI 도구 정보 로드 중 오류 발생:', error);
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(error, 'AI 도구 정보 로드');
                } else {
                    console.error('AI 도구 정보 로드 오류:', error);
                }
            }
        }

        // 레시피 데이터 로드 함수 (페이지네이션 지원)
        async function loadRecipeData(page = 1, category = null, searchTerm = '') {
            console.log('loadRecipeData 함수 호출됨:', { page, category, searchTerm });
            
            if (isLoading) {
                console.log('이미 로딩 중입니다.');
                return;
            }
            
            // Firebase 연결 상태 확인
            if (!window.db) {
                console.error('Firebase DB가 초기화되지 않았습니다.');
                showLoading(false);
                return;
            }
            
            try {
                isLoading = true;
                showLoading(true);
                console.log('레시피 데이터 로드 시작...');
                
                // 먼저 AI 도구 이미지들을 로드 (첫 페이지에서만)
                if (page === 1) {
                    console.log('AI 도구 이미지 로드 시작...');
                    await loadToolImages();
                }
                
                console.log('Firestore 컬렉션에 접근 중...');
                const recipesRef = collection(window.db, 'public_recipe_collection');
                
                // 먼저 단순한 쿼리로 테스트 (정렬 없이)
                let q = query(recipesRef, limit(itemsPerPage));
                console.log('기본 쿼리 생성:', q);
                
                // 카테고리 필터 적용
                if (category && category !== 'All Categories') {
                    console.log('카테고리 필터 적용:', category);
                    q = query(q, where('my_recipe_category', '==', category));
                }
                
                // 검색어가 있는 경우 필터링
                if (searchTerm) {
                    console.log('검색어 필터 적용:', searchTerm);
                    // 검색은 클라이언트 사이드에서 처리 (Firestore 제한 때문)
                    q = query(q, limit(itemsPerPage));
                }
                
                // 페이지네이션 적용 (startAfter는 나중에 구현)
                console.log('최종 쿼리:', q);
                
                console.log('Firestore 쿼리 실행 중...');
                const querySnapshot = await getDocs(q);
                console.log('쿼리 결과:', querySnapshot.size, '개의 문서');
                
                // 쿼리 결과 상세 분석
                if (querySnapshot.empty) {
                    console.log('쿼리 결과가 비어있습니다. 컬렉션 존재 여부 확인 중...');
                    
                    // 컬렉션 자체가 존재하는지 확인
                    try {
                        const testQuery = query(collection(window.db, 'public_recipe_collection'), limit(1));
                        const testSnapshot = await getDocs(testQuery);
                        console.log('테스트 쿼리 결과:', testSnapshot.size, '개의 문서');
                        
                        if (testSnapshot.empty) {
                            console.error('public_recipe_collection 컬렉션이 비어있거나 접근할 수 없습니다.');
                        } else {
                            console.log('컬렉션은 존재하지만 쿼리 조건에 맞는 문서가 없습니다.');
                        }
                    } catch (testError) {
                        console.error('테스트 쿼리 오류:', testError);
                    }
                } else {
                    console.log('쿼리 성공! 문서 ID들:', querySnapshot.docs.map(doc => doc.id));
                }
                
                const workflowsContainer = document.getElementById('workflowsGrid');
                if (!workflowsContainer) {
                    console.error('워크플로우 컨테이너를 찾을 수 없습니다.');
                    return;
                }

                // 첫 페이지인 경우에만 기존 카드들 제거
                if (page === 1) {
                    workflowsContainer.innerHTML = '';
                    allRecipes = [];
                }

                // 새로 로드된 레시피 데이터 저장
                const newRecipes = [];
                querySnapshot.forEach((doc) => {
                    const data = doc.data();
                    data.docId = doc.id;
                    newRecipes.push(data);
                    allRecipes.push(data);
                    console.log('레시피 데이터:', {
                        id: doc.id,
                        title: data.my_recipe_title || '제목 없음',
                        category: data.my_recipe_category || '카테고리 없음',
                        description: data.my_recipe_basic_description || '설명 없음'
                    });
                });

                // 검색어가 있는 경우 필터링
                if (searchTerm) {
                    filteredRecipes = allRecipes.filter(recipe => {
                        const title = (recipe.my_recipe_title || '').toLowerCase();
                        const description = (recipe.my_recipe_basic_description || '').toLowerCase();
                        const tools = (recipe.my_recipe_tool || []).join(' ').toLowerCase();
                        const category = (recipe.my_recipe_category || '').toLowerCase();
                        const difficulty = (recipe.my_recipe_difficulty || '').toLowerCase();
                        const recommendedGroups = (recipe.my_recipe_recommended_groups || []).join(' ').toLowerCase();

                        return title.includes(searchTerm) ||
                               description.includes(searchTerm) ||
                               tools.includes(searchTerm) ||
                               category.includes(searchTerm) ||
                               difficulty.includes(searchTerm) ||
                               recommendedGroups.includes(searchTerm);
                    });
                } else {
                    filteredRecipes = [...allRecipes];
                }

                // 페이지네이션 정보 업데이트
                currentPage = page;
                totalPages = Math.ceil(filteredRecipes.length / itemsPerPage);
                hasMoreData = newRecipes.length === itemsPerPage;
                
                console.log('필터링된 레시피 수:', filteredRecipes.length);
                console.log('현재 페이지:', currentPage, '/', totalPages);
                
                // 카드들 렌더링
                if (page === 1) {
                    renderRecipes(filteredRecipes.slice(0, itemsPerPage));
                } else {
                    appendRecipes(newRecipes);
                }
                
                // 페이지네이션 UI 업데이트
                updatePaginationUI();
                
                console.log(`페이지 ${page}: ${newRecipes.length}개의 레시피를 로드했습니다.`);
                
                // 첫 페이지에서만 검색 이벤트 리스너 설정
                if (page === 1) {
                    setupSearchListener();
                    setupPaginationListeners();
                    setupInfiniteScroll();
                }
                
            } catch (error) {
                console.error('레시피 데이터 로드 중 오류 발생:', error);
                if (window.CommonUtils) {
                    window.CommonUtils.handleError(error, '레시피 데이터 로드');
                } else {
                    console.error('레시피 데이터 로드 오류:', error);
                }
                
                // 오류 발생 시 사용자에게 알림
                const workflowsContainer = document.getElementById('workflowsGrid');
                if (workflowsContainer) {
                    workflowsContainer.innerHTML = `
                        <div class="col-span-full text-center py-8" style="color: var(--text-secondary);">
                            <i class="fas fa-exclamation-triangle text-4xl mb-4 text-red-500"></i>
                            <p class="text-lg">데이터 로드 중 오류가 발생했습니다.</p>
                            <p class="text-sm mt-2">페이지를 새로고침하거나 잠시 후 다시 시도해주세요.</p>
                            <button onclick="window.location.reload()" class="btn-primary mt-4 px-4 py-2">
                                페이지 새로고침
                            </button>
                        </div>
                    `;
                }
            } finally {
                isLoading = false;
                showLoading(false);
            }
        }

        // 전역에서 호출할 수 있도록 노출
        window.loadRecipeData = loadRecipeData;
        
        // 페이지 로드 완료 후 자동으로 데이터 로드 시도
        function attemptAutoLoad() {
            console.log('자동 데이터 로드 시도...');
            if (window.db && window.loadRecipeData) {
                console.log('자동으로 레시피 데이터 로드 시작');
                window.loadRecipeData();
            } else {
                console.log('Firebase 또는 loadRecipeData 함수가 준비되지 않음, 2초 후 재시도');
                setTimeout(attemptAutoLoad, 2000);
            }
        }
        
        // 페이지 로드 완료 후 자동 로드 시도
        if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', () => {
                setTimeout(attemptAutoLoad, 1000);
            });
        } else {
            setTimeout(attemptAutoLoad, 1000);
        }
        
        // 로딩 상태 표시/숨김 함수
        function showLoading(show) {
            const loadingIndicator = document.getElementById('loadingIndicator');
            if (loadingIndicator) {
                if (show) {
                    loadingIndicator.classList.remove('hidden');
                } else {
                    loadingIndicator.classList.add('hidden');
                }
            }
        }
        
        // 페이지네이션 UI 업데이트 함수
        function updatePaginationUI() {
            const prevBtn = document.getElementById('prevPageBtn');
            const nextBtn = document.getElementById('nextPageBtn');
            const pageInfo = document.getElementById('pageInfo');
            const loadMoreBtn = document.getElementById('loadMoreBtn');
            
            if (prevBtn) prevBtn.disabled = currentPage <= 1;
            if (nextBtn) nextBtn.disabled = currentPage >= totalPages;
            if (pageInfo) pageInfo.textContent = `${currentPage} / ${totalPages}`;
            if (loadMoreBtn) loadMoreBtn.style.display = hasMoreData ? 'inline-block' : 'none';
        }
        
        // 페이지네이션 이벤트 리스너 설정
        function setupPaginationListeners() {
            const prevBtn = document.getElementById('prevPageBtn');
            const nextBtn = document.getElementById('nextPageBtn');
            const loadMoreBtn = document.getElementById('loadMoreBtn');
            
            if (prevBtn) {
                prevBtn.addEventListener('click', () => {
                    if (currentPage > 1) {
                        loadRecipeData(currentPage - 1);
                    }
                });
            }
            
            if (nextBtn) {
                nextBtn.addEventListener('click', () => {
                    if (currentPage < totalPages) {
                        loadRecipeData(currentPage + 1);
                    }
                });
            }
            
            if (loadMoreBtn) {
                loadMoreBtn.addEventListener('click', () => {
                    loadMoreRecipes();
                });
            }
        }
        
        // 무한 스크롤 설정
        function setupInfiniteScroll() {
            let scrollTimeout;
            
            window.addEventListener('scroll', () => {
                clearTimeout(scrollTimeout);
                scrollTimeout = setTimeout(() => {
                    if (isNearBottom() && hasMoreData && !isLoading) {
                        loadMoreRecipes();
                    }
                }, 100);
            });
        }
        
        // 페이지 하단 근처인지 확인
        function isNearBottom() {
            const scrollTop = window.pageYOffset || document.documentElement.scrollTop;
            const windowHeight = window.innerHeight;
            const documentHeight = document.documentElement.scrollHeight;
            
            return scrollTop + windowHeight >= documentHeight - 100;
        }
        
        // 더 많은 레시피 로드
        async function loadMoreRecipes() {
            if (isLoading || !hasMoreData) return;
            
            try {
                isLoading = true;
                showLoading(true);
                
                const recipesRef = collection(window.db, 'public_recipe_collection');
                const startIndex = allRecipes.length;
                const q = query(recipesRef, orderBy('created_at', 'desc'), limit(itemsPerPage), startAfter(startIndex));
                
                const querySnapshot = await getDocs(q);
                
                if (querySnapshot.empty) {
                    hasMoreData = false;
                    return;
                }
                
                const newRecipes = [];
                querySnapshot.forEach((doc) => {
                    const data = doc.data();
                    data.docId = doc.id;
                    newRecipes.push(data);
                    allRecipes.push(data);
                });
                
                // 새 레시피들을 기존 그리드에 추가
                appendRecipes(newRecipes);
                
                // 페이지네이션 정보 업데이트
                hasMoreData = newRecipes.length === itemsPerPage;
                updatePaginationUI();
                
                console.log(`추가 로드: ${newRecipes.length}개의 레시피`);
                
            } catch (error) {
                console.error('추가 레시피 로드 오류:', error);
            } finally {
                isLoading = false;
                showLoading(false);
            }
        }
        
        // 레시피들을 기존 그리드에 추가
        function appendRecipes(newRecipes) {
            const workflowsContainer = document.getElementById('workflowsGrid');
            if (!workflowsContainer) return;
            
            newRecipes.forEach((recipeData) => {
                window.__recipesById[recipeData.docId] = recipeData;
                const workflowCard = createWorkflowCard(recipeData, recipeData.docId);
                workflowsContainer.appendChild(workflowCard);
            });
        }

        // 레시피 렌더링 함수
        function renderRecipes(recipes) {
            const workflowsContainer = document.getElementById('workflowsGrid');
            if (!workflowsContainer) return;

            workflowsContainer.innerHTML = '';

            if (recipes.length === 0) {
                workflowsContainer.innerHTML = `
                    <div class="col-span-full text-center py-8" style="color: var(--text-secondary);">
                        <i class="fas fa-search text-4xl mb-4 opacity-50"></i>
                        <p class="text-lg">검색 결과가 없습니다.</p>
                        <p class="text-sm mt-2">다른 키워드로 검색해보세요.</p>
                    </div>
                `;
                return;
            }

            recipes.forEach((recipeData) => {
                window.__recipesById[recipeData.docId] = recipeData;
                const workflowCard = createWorkflowCard(recipeData, recipeData.docId);
                workflowsContainer.appendChild(workflowCard);
            });
        }

        // 검색 이벤트 리스너 설정
        function setupSearchListener() {
            const searchInput = document.getElementById('searchInput');
            const clearSearchBtn = document.getElementById('clearSearchBtn');
            if (!searchInput) return;

            let searchTimeout;
            
            searchInput.addEventListener('input', function() {
                clearTimeout(searchTimeout);
                
                // X 버튼 표시/숨김 처리
                if (this.value.length > 0) {
                    clearSearchBtn.classList.remove('hidden');
                } else {
                    clearSearchBtn.classList.add('hidden');
                }
                
                searchTimeout = setTimeout(() => {
                    const searchTerm = this.value.toLowerCase().trim();
                    performSearch(searchTerm);
                }, 300); // 300ms 딜레이로 성능 최적화
            });

            // 엔터 키 이벤트
            searchInput.addEventListener('keypress', function(e) {
                if (e.key === 'Enter') {
                    const searchTerm = this.value.toLowerCase().trim();
                    performSearch(searchTerm);
                }
            });
        }

        // 검색 실행 함수 (페이지네이션 지원)
        function performSearch(searchTerm) {
            const searchResultsCount = document.getElementById('searchResultsCount');
            const resultsCount = document.getElementById('resultsCount');

            if (!searchTerm) {
                // 검색어가 없으면 모든 레시피 표시
                filteredRecipes = [...allRecipes];
                searchResultsCount.style.display = 'none';
                currentPage = 1;
                totalPages = Math.ceil(filteredRecipes.length / itemsPerPage);
            } else {
                // 검색어가 있으면 필터링
                filteredRecipes = allRecipes.filter(recipe => {
                    const title = (recipe.my_recipe_title || '').toLowerCase();
                    const description = (recipe.my_recipe_basic_description || '').toLowerCase();
                    const tools = (recipe.my_recipe_tool || []).join(' ').toLowerCase();
                    const category = (recipe.my_recipe_category || '').toLowerCase();
                    const difficulty = (recipe.my_recipe_difficulty || '').toLowerCase();
                    const recommendedGroups = (recipe.my_recipe_recommended_groups || []).join(' ').toLowerCase();

                    return title.includes(searchTerm) ||
                           description.includes(searchTerm) ||
                           tools.includes(searchTerm) ||
                           category.includes(searchTerm) ||
                           difficulty.includes(searchTerm) ||
                           recommendedGroups.includes(searchTerm);
                });

                // 검색 결과 카운트 표시
                searchResultsCount.style.display = 'block';
                resultsCount.textContent = filteredRecipes.length;
                
                // 검색 시 첫 페이지로 리셋
                currentPage = 1;
                totalPages = Math.ceil(filteredRecipes.length / itemsPerPage);
            }

            // 필터링된 레시피 렌더링 (첫 페이지만)
            const firstPageRecipes = filteredRecipes.slice(0, itemsPerPage);
            renderRecipes(firstPageRecipes);
            
            // 페이지네이션 UI 업데이트
            updatePaginationUI();
        }

        // 검색 초기화 함수
        window.clearSearch = function() {
            const searchInput = document.getElementById('searchInput');
            const clearSearchBtn = document.getElementById('clearSearchBtn');
            const searchResultsCount = document.getElementById('searchResultsCount');
            
            if (searchInput) {
                searchInput.value = '';
                searchInput.focus();
            }
            
            if (clearSearchBtn) {
                clearSearchBtn.classList.add('hidden');
            }
            
            if (searchResultsCount) {
                searchResultsCount.style.display = 'none';
            }
            
            // 모든 레시피 표시
            filteredRecipes = [...allRecipes];
            const firstPageRecipes = filteredRecipes.slice(0, itemsPerPage);
            renderRecipes(firstPageRecipes);
            
            // 페이지네이션 리셋
            currentPage = 1;
            totalPages = Math.ceil(filteredRecipes.length / itemsPerPage);
            updatePaginationUI();
        };
        
        // 카테고리 변경 이벤트 핸들러
        window.onCategoryChange = function(category) {
            // 검색어 초기화
            const searchInput = document.getElementById('searchInput');
            if (searchInput) {
                searchInput.value = '';
            }
            
            // 카테고리별 지연 로딩 큐에 추가
            categoryLoadQueue.push(category);
            
            // 이미 로딩 중이 아니라면 큐 처리 시작
            if (!isCategoryLoading) {
                processCategoryQueue();
            }
        };
        
        // 카테고리 로딩 큐 처리
        async function processCategoryQueue() {
            if (isCategoryLoading || categoryLoadQueue.length === 0) return;
            
            isCategoryLoading = true;
            
            while (categoryLoadQueue.length > 0) {
                const category = categoryLoadQueue.shift();
                
                // 현재 페이지를 1로 리셋하고 새 카테고리로 데이터 로드
                currentPage = 1;
                allRecipes = [];
                filteredRecipes = [];
                
                try {
                    await loadRecipeData(1, category);
                } catch (error) {
                    console.error('카테고리 변경 오류:', error);
                }
                
                // 카테고리 간 전환 시 약간의 지연 (사용자 경험 개선)
                if (categoryLoadQueue.length > 0) {
                    await new Promise(resolve => setTimeout(resolve, 300));
                }
            }
            
            isCategoryLoading = false;
        }

        // 워크플로우 카드 생성 함수
        function createWorkflowCard(recipeData, docId) {
            const card = document.createElement('div');
            card.className = 'workflow-card p-3 sm:p-4';
            
            // 카드 클릭 이벤트 추가
            card.style.cursor = 'pointer';
            card.addEventListener('click', function(e) {
                // 버튼 클릭 시에는 상세 페이지로 이동하지 않음
                if (e.target.closest('button')) {
                    return;
                }
                // 상세 페이지로 이동
                window.location.href = `ai_comb_detail.html?id=${docId}`;
            });
            
            // 사용자 아바타 (첫 글자 사용)
            const userName = recipeData.user_id ? recipeData.user_id.substring(0, 1) : 'U';
            const avatarColor = getAvatarColor(recipeData.user_id);
            
            // 도구 아이콘과 이름 생성 (최대 4개까지 표시, 초과 시 3개만 표시)
            const tools = recipeData.my_recipe_tool || [];
            const maxTools = tools.length > 4 ? 3 : 4;
            const displayTools = tools.slice(0, maxTools);
            let toolElements = displayTools.map((tool, index) => {
                const toolImage = toolImagesCache[tool];
                const bgColor = getToolColor(tool);
                
                let toolIcon;
                if (toolImage) {
                    // DB에서 가져온 이미지가 있는 경우 (에러 시 이미지만 숨김)
                    toolIcon = `
                        <div class="tool-icon ${bgColor}" style="padding: 2px;">
                            <img src="${toolImage}" alt="${tool}" style="width: 100%; height: 100%; object-fit: contain; border-radius: 4px; max-width: 100%; max-height: 100%;" onerror="this.style.display='none';">
                        </div>
                    `;
                } else {
                    // 기본 아이콘 사용
                    const iconClass = getToolIcon(tool);
                    toolIcon = `
                        <div class="tool-icon ${bgColor}">
                            <i class="${iconClass}"></i>
                        </div>
                    `;
                }
                
                return `
                    <div class="tool-container">
                        ${toolIcon}
                        <div class="tool-name">${tool}</div>
                    </div>
                    ${index < displayTools.length - 1 ? '<i class="fas fa-arrow-right arrow mx-1"></i>' : ''}
                `;
            }).join('');

            // 4개를 초과하는 도구가 있을 경우 추가 표시
            const additionalToolsCount = tools.length - maxTools;
            if (tools.length > 4) {
                toolElements += `
                    <i class="fas fa-arrow-right arrow mx-1"></i>
                    <div class="tool-container">
                        <div class="tool-icon bg-gray-500 flex items-center justify-center">
                            <span class="text-xs font-bold">+${additionalToolsCount}</span>
                        </div>
                        <div class="tool-name">추가</div>
                    </div>
                `;
            }

            // 도구 이름 문자열 (더 이상 사용하지 않음)
            // const toolNames = tools.join(' → ');

            card.innerHTML = `
                <div class="flex items-center mb-3">
                    <div class="user-avatar ${avatarColor} mr-2">
                        <span>${userName}</span>
                    </div>
                    <span class="font-semibold text-sm truncate">${recipeData.user_id ? recipeData.user_id.substring(0, 6) : 'Unknown'}</span>
                </div>
                
                <div class="text-left mb-3">
                    <h3 class="text-base font-semibold mb-1 line-clamp-2" style="display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden; text-overflow: ellipsis; line-height: 1.3;">${recipeData.my_recipe_title || 'Untitled Recipe'}</h3>
                    <p class="text-xs text-gray-500" style="display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; overflow: hidden; text-overflow: ellipsis; line-height: 1.3;">${recipeData.my_recipe_basic_description || ''}</p>
                </div>
                
                <div class="flex items-center justify-center mb-3 overflow-hidden">
                    ${toolElements}
                </div>
                
                <div class="flex items-center justify-center space-x-4 text-xs mb-3">
                    <div class="flex items-center">
                        <i class="fas fa-heart text-red-500 mr-1"></i>
                        <span>${recipeData.likes_count || 0}</span>
                    </div>
                    <div class="flex items-center">
                        <i class="fas fa-comment text-blue-500 mr-1"></i>
                        <span>${recipeData.comments_count || 0}</span>
                    </div>
                    <div class="flex items-center">
                        <i class="fas fa-bookmark text-yellow-500 mr-1"></i>
                        <span>${recipeData.bookmarks_count || 0}</span>
                    </div>
                </div>
                
                <div class="flex justify-center">
                    <button class="btn-primary text-xs px-3 py-2 w-full" onclick="event.stopPropagation(); showRecipeSummary('${docId}')">
                        레시피 요약 보기
                    </button>
                </div>
            `;

            return card;
        }

        // 아바타 색상 생성 함수
        function getAvatarColor(userId) {
            const colors = ['bg-purple-500', 'bg-blue-500', 'bg-red-500', 'bg-yellow-500', 'bg-pink-500', 'bg-green-500'];
            if (!userId) return colors[0];
            const hash = userId.split('').reduce((a, b) => {
                a = ((a << 5) - a) + b.charCodeAt(0);
                return a & a;
            }, 0);
            return colors[Math.abs(hash) % colors.length];
        }

        // 도구 아이콘 매핑 함수
        function getToolIcon(toolName) {
            const iconMap = {
                'ChatGPT': 'fas fa-comments',
                'Notion AI': 'fas fa-note-sticky',
                'Midjourney': 'fas fa-palette',
                'Claude': 'fas fa-robot',
                'Google Sheets': 'fas fa-table',
                'Zapier': 'fas fa-bolt',
                'Whisper': 'fas fa-microphone-lines',
                'Descript': 'fas fa-edit',
                'Adobe Podcast': 'fas fa-podcast',
                'Hugging Face': 'fas fa-face-smile',
                'Grammarly': 'fas fa-check',
                'Copy.ai': 'fas fa-copy',
                'DALL-E': 'fas fa-image',
                'Upscayl': 'fas fa-magic',
                'Photoshop': 'fas fa-paint-brush',
                'GitHub Copilot': 'fas fa-code',
                'Anyword': 'fas fa-pen',
                'AIVA': 'fas fa-music',
                'Canva': 'fas fa-palette'
            };
            return iconMap[toolName] || 'fas fa-cog';
        }

        // 도구 색상 매핑 함수
        function getToolColor(toolName) {
            const colorMap = {
                'ChatGPT': 'bg-green-500',
                'Notion AI': 'bg-black',
                'Midjourney': 'bg-blue-500',
                'Claude': 'bg-orange-400',
                'Google Sheets': 'bg-green-600',
                'Zapier': 'bg-orange-500',
                'Whisper': 'bg-blue-400',
                'Descript': 'bg-blue-600',
                'Adobe Podcast': 'bg-red-500',
                'Hugging Face': 'bg-yellow-400',
                'Grammarly': 'bg-green-500',
                'Copy.ai': 'bg-green-600',
                'DALL-E': 'bg-purple-500',
                'Upscayl': 'bg-blue-500',
                'Photoshop': 'bg-red-500',
                'GitHub Copilot': 'bg-gray-800',
                'Anyword': 'bg-blue-600',
                'AIVA': 'bg-purple-600',
                'Canva': 'bg-blue-400'
            };
            return colorMap[toolName] || 'bg-gray-500';
        }

        // 레시피 요약 표시 함수
        window.showRecipeSummary = function(docId) {
            const modal = document.getElementById('recipeSummaryModal');
            const title = document.getElementById('recipeSummaryTitle');
            const content = document.getElementById('recipeSummaryContent');
            const recipeData = window.__recipesById ? window.__recipesById[docId] : null;
            if (!recipeData) return;
            
            // 제목 설정
            title.textContent = recipeData.my_recipe_title || '레시피 요약';
            
            // 난이도 태그 생성
            const difficultyTag = recipeData.my_recipe_difficulty ? 
                `<span class="inline-block bg-pink-100 dark:bg-pink-900 text-pink-800 dark:text-pink-200 text-xs px-2 py-1 rounded-full mr-2" style="background-color: #ec4899; color: #ffffff;">${getDifficultyText(recipeData.my_recipe_difficulty)}</span>` : '';
            
            // 카테고리 태그 생성
            const categoryTag = recipeData.my_recipe_category ? 
                `<span class="inline-block bg-green-100 dark:bg-green-900 text-green-800 dark:text-green-200 text-xs px-2 py-1 rounded-full" style="background-color: #10b981; color: #ffffff;">${getCategoryText(recipeData.my_recipe_category)}</span>` : '';
            
            // 추천 그룹 태그 생성
            const recommendedGroups = recipeData.my_recipe_recommended_groups || [];
            const groupTags = recommendedGroups.map(group => 
                `<span class="inline-block bg-yellow-100 dark:bg-yellow-900 text-yellow-800 dark:text-yellow-200 text-xs px-2 py-1 rounded mr-2" style="background-color: #f59e0b; color: #ffffff;">${getGroupText(group)}</span>`
            ).join('');
            
            // 도구 정1보 생성 - DB에서 실제 도구 정보를 가져옴
            const tools = recipeData.my_recipe_tool || [];
            const toolDescriptions = tools.map(tool => {
                return `
                    <div class="mb-4 p-3 bg-gray-50 dark:bg-gray-700 rounded-lg border border-gray-200 dark:border-gray-600" style="background-color: var(--hover-bg); border-color: var(--border-color);">
                        <div class="flex items-center mb-2">
                            <div class="w-8 h-8 ${getToolColor(tool)} rounded-lg flex items-center justify-center mr-3">
                                <i class="${getToolIcon(tool)} text-white text-sm"></i>
                            </div>
                            <h4 class="font-semibold text-gray-800 dark:text-gray-200" style="color: var(--text-primary);">${tool}</h4>
                        </div>
                        <p class="text-sm text-gray-600 dark:text-gray-400" style="color: var(--text-secondary);">${getToolInfo(tool).description}</p>
                    </div>
                `;
            }).join('');
            
            // 콘텐츠 설정
            content.innerHTML = `
                <div class="mb-4" style="color: var(--text-primary);">
                    <h3 class="text-lg font-semibold mb-2" style="color: var(--text-primary);">난의도 및 카테고리</h3>
                    <div class="flex flex-wrap gap-2 mb-3">
                        ${difficultyTag}
                        ${categoryTag}
                    </div>
                    <p class="text-gray-700 dark:text-gray-300 mb-4" style="color: var(--text-secondary);">${recipeData.my_recipe_basic_description || '설명이 없습니다.'}</p>
                </div>
                
                ${groupTags ? `
                <div class="mb-4" style="color: var(--text-primary);">
                    <h3 class="text-lg font-semibold text-gray-800 dark:text-gray-200 mb-2" style="color: var(--text-primary);">추천 사용자 그룹</h3>
                    <div class="flex flex-wrap gap-2">
                        ${groupTags}
                    </div>
                </div>
                ` : ''}
                
                <div class="mb-6" style="color: var(--text-primary);">
                    <h3 class="text-lg font-semibold text-gray-800 dark:text-gray-200 mb-3" style="color: var(--text-primary);">사용할 AI 도구:</h3>
                    ${toolDescriptions}
                </div>
                
                <div class="flex justify-center">
                    <button onclick="showDetailedRecipe('${docId}')" class="btn-primary px-6 py-2">
                        상세 레시피 정보 보기
                    </button>
                </div>
            `;
            
            // 모달 표시
            modal.classList.remove('hidden');
            modal.classList.add('flex');
        };

        // 레시피 요약 닫기 함수
        window.closeRecipeSummary = function() {
            const modal = document.getElementById('recipeSummaryModal');
            modal.classList.add('hidden');
            modal.classList.remove('flex');
        };

        // 상세 레시피 보기 함수
        window.showDetailedRecipe = function(docId) {
            // 상세 레시피 페이지로 이동
            window.location.href = `ai_comb_detail.html?id=${docId}`;
        };

        // 난이도 텍스트 변환 함수
        function getDifficultyText(difficulty) {
            const difficultyMap = {
                'beginner': '초급',
                'intermediate': '중급',
                'advanced': '고급'
            };
            return difficultyMap[difficulty] || difficulty;
        }

        // 카테고리 텍스트 변환 함수
        function getCategoryText(category) {
            const categoryMap = {
                'text-generation': '텍스트 생성',
                'image-generation': '이미지 생성',
                'audio-generation': '음성/오디오',
                'video-generation': '비디오 제작',
                'coding-assistant': '코드 작성',
                'data-analysis': '데이터 분석',
                'marketing': '홍보 / 마케팅',
                'productivity': '생산성',
                'design': '디자인',
                'education': '교육',
                'business': '비즈니스',
                'other': '기타'
            };
            return categoryMap[category] || category;
        }

        // 그룹 텍스트 변환 함수
        function getGroupText(group) {
            const groupMap = {
                'office-worker': '직장인',
                'university-student': '대학생',
                'professional': '전문직',
                'video-audio': '영상/음성 관련',
                'marketing': '마케팅 직군',
                'management-support': '경영 지원',
                'researcher': '연구원',
                'student': '중고생',
                'developer': '개발직군',
                'medical': '의료 관련',
                'design': '디자인',
                'c-level': 'C level 임원'
            };
            return groupMap[group] || group;
        }

        // 도구 정보 가져오기 함수 - DB에서 실제 도구 정보를 가져옴
        function getToolInfo(toolName) {
            // DB에서 로드된 도구 정보 캐시에서 찾기
            if (toolInfoCache && toolInfoCache[toolName]) {
                return toolInfoCache[toolName];
            }
            
            // 기본 도구 정보 (DB에서 가져올 수 없는 경우를 위한 fallback)
            const toolInfoMap = {
                'ChatGPT': {
                    description: '대화형 AI 언어 모델로 질문에 답변하고 다양한 콘텐츠를 생성합니다. 텍스트 작성, 아이디어 생성, 문제 해결 등에 활용할 수 있습니다.'
                },
                'Midjourney': {
                    description: '텍스트 설명을 기반으로 고품질 이미지를 생성하는 AI 도구입니다. 아트워크, 일러스트레이션, 디자인 요소 등을 제작할 수 있습니다.'
                },
                'Jasper': {
                    description: '마케팅 및 콘텐츠 제작을 위한 전문 AI 글쓰기 도구입니다. 블로그 포스트, 광고 카피, 소셜미디어 콘텐츠 등을 작성할 수 있습니다.'
                },
                'Canva': {
                    description: '직관적인 디자인 도구로 템플릿을 활용하여 그래픽 디자인을 쉽게 제작할 수 있습니다. 로고, 배너, 프레젠테이션 등을 만들 수 있습니다.'
                },
                'Anyword': {
                    description: 'AI 기반 마케팅 카피라이팅 도구로 광고 문구, 이메일 제목, 소셜미디어 포스트 등을 최적화하여 작성할 수 있습니다.'
                },
                'AIVA': {
                    description: 'AI 음악 생성 도구로 텍스트 설명이나 감정을 바탕으로 맞춤형 음악을 제작할 수 있습니다. 배경음악, 브랜드 음악 등에 활용됩니다.'
                },
                'Descript': {
                    description: '오디오 및 비디오 편집을 위한 AI 기반 도구입니다. 음성 인식, 자동 자막 생성, 오디오 편집 등을 지원합니다.'
                },
                'Speechify': {
                    description: '텍스트를 자연스러운 음성으로 변환하는 AI 도구입니다. 다양한 언어와 음성 옵션을 제공합니다.'
                }
            };
            return toolInfoMap[toolName] || {
                description: `${toolName}은 다양한 작업을 지원하는 AI 도구입니다.`
            };
        }
    </script>
    <!-- 불필요한 스크립트 제거 -->


