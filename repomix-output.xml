                        </td>
                        <td class="px-6 py-4">
                            <div class="text-sm text-gray-200">
                                ${(Array.isArray(tool.featuresKr) && tool.featuresKr.length > 0)
                                    ? tool.featuresKr.map(f => `<div><span style='font-size:0.7em;vertical-align:middle;'>â—</span> ${f}</div>`).join('')
                                    : ''}
                            </div>
                        </td>
                        <td class="px-6 py-4">
                            <div class="flex flex-wrap">
                                ${(tool.tagsKr || []).map((tag, idx) => {
                                    const colorClass = tagColors[idx % tagColors.length];
