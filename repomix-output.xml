<script>
function displaySearchResults(tools) {
    const container = document.getElementById('searchResultsContainer');
    
    if (!tools || tools.length === 0) {
        container.innerHTML = `
            <div class="col-span-full text-center py-4">
                <p class="text-gray-400">검색 결과가 없습니다.</p>
            </div>
        `;
        return;
    }

    container.innerHTML = tools.map(tool => `
        <div class="bg-[#121620] rounded-xl p-6 flex flex-col cursor-pointer hover:bg-opacity-80 transition-all duration-300" onclick="displayToolDetails('${tool.id}')">
            <div class="flex items-start justify-between">
                <div class="flex items-start gap-3">
                    <div class="w-10 h-10 bg-[#1E293B] rounded-xl flex items-center justify-center flex-shrink-0">
