            // ëª¨ë°”ì¼ ì¹´ë“œ ë¦¬ìŠ¤íŠ¸ ë Œë”ë§ ì¶”ê°€ (í˜ì´ì§• ì ìš©)
            const cardList = document.getElementById('tools-card-list');
