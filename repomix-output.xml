// Firebase 초기화 상태 확인
function checkFirebaseReady() {
    return new Promise((resolve) => {
        if (window.db && window.auth) {
            resolve(true);
        } else {
            // Firebase 초기화 대기
            const checkInterval = setInterval(() => {
                if (window.db && window.auth) {
                    clearInterval(checkInterval);
                    resolve(true);
                }
            }, 100);
            
            // 10초 후 타임아웃
            setTimeout(() => {
                clearInterval(checkInterval);
                resolve(false);
            }, 10000);
        }
    });
}

