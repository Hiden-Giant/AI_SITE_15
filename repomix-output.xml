(function() {
  function tryAttachAuthListener() {
    // AuthManagerê°€ ì´ˆê¸°í™”ë˜ì—ˆëŠ”ì§€ í™•ì¸
    if (window.authManager && window.authManager.auth && window.authManager.auth.onAuthStateChanged) {
